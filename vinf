spawn(function()
game.RunService.RenderStepped:connect(function()
    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
end)
end)

plr.SimulationRadiusChanged:Connect(function(radius)
    radius = 9e9
    return radius
end)

local MT = getrawmetatable(game)
local OldNameCall = MT.__namecall
setreadonly(MT, false)
MT.__namecall = newcclosure(function(self, ...)
    local Method = getnamecallmethod()
    local Args = {...}

    if Method == 'FireServer' and self.Name == 'RemoteEvent' then
        if getgenv().mastery or atoaimbot or getgenv().masteryGun or Settingss.atofarmbounty or getgenv().AutoCore then
            Args[1] = getgenv().psskill
        end
    end

    return OldNameCall(self, unpack(Args))
end)

setreadonly(MT, true)

function NoclipLoop()
    if game.Players.LocalPlayer.Character ~= nil then
        for _, child in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if child:IsA("BasePart") and child.CanCollide == true then
                child.CanCollide = false
            end
        end
    end
end
function CreateTweenFloat()
    if not plr.Character.HumanoidRootPart:FindFirstChild("EffectsSY") then
        local BV = Instance.new("BodyVelocity")
        BV.Parent = plr.Character.HumanoidRootPart
        BV.Name = "EffectsSY"
        BV.MaxForce = Vector3.new(100000, 100000, 100000)
        BV.Velocity = Vector3.new(0, 0, 0)
    end
end
getgenv().starttweenisland=false
game:GetService('RunService').Stepped:connect(function()
    if getgenv().starttweenisland or atofarmcandy or Settingss.atocavander or Settingss.atobuddysword  or Settingss.atofarmhakiken or  Settingss.atosharkmanv2 or Settingss.atodeadstep or Settingss.atopolehop or atoRengoku or atosaber or Settingss.atoraidhop or getgenv().AutoRaid or getgenv().Farm2 or getgenv().mastery or getgenv().atobartooo or  getgenv().noclip or Settingss.autosea3 or getgenv().masteryGun or Settingss.hakikenhop  or  getgenv().Boss or atofarmmoball or atoelite or atohakikenv2 or Settingss.atoelitehop or atoracev2 or atocitizen or atolongma or Settingss.atofarmbounty or Settingss.atoindra or atoplayer or atofarmbossall or atofarmhakiken or Settingss.atofully then 
        if plr.Character:FindFirstChild("Humanoid") and plr.Character.Humanoid.Health ~= 0 then
            NoclipLoop()
        end
    end
end)
game:GetService('RunService').Stepped:connect(function()
    if getgenv().starttweenisland or Settingss.atodeadstep or atofarmcandy or Settingss.atobuddysword or Settingss.atocavander or Settingss.atofarmhakiken or Settingss.atosharkmanv2 or Settingss.atopolehop  or atoRengoku or atosaber or Settingss.atoraidhop or getgenv().AutoRaid or getgenv().Farm2 or getgenv().mastery or getgenv().atobartooo or  getgenv().noclip or Settingss.autosea3 or getgenv().masteryGun or Settingss.hakikenhop  or  getgenv().Boss or atofarmmoball or atoelite or atohakikenv2 or Settingss.atoelitehop or atoracev2 or atocitizen or atolongma or Settingss.atofarmbounty or Settingss.atoindra or atoplayer or atofarmbossall or atofarmhakiken or Settingss.atofully then 
        if plr.Character:FindFirstChild("Humanoid") and plr.Character.Humanoid.Health ~= 0 and not plr.Character.HumanoidRootPart:FindFirstChild("EffectsSY")  then
            CreateTweenFloat()
        end
    end
end)
game:GetService('RunService').Stepped:connect(function()
    if not getgenv().starttweenisland and not Settingss.atodeadstep and not atofarmcandy and not Settingss.atobuddysword and not Settingss.atocavander and not Settingss.atofarmhakiken and not  Settingss.atosharkmanv2 and not Settingss.atopolehop  and not atosaber and not atoRengoku and not Settingss.atoraidhop and not getgenv().AutoRaid and not getgenv().Farm2 and not getgenv().mastery and not getgenv().atobartooo and not  getgenv().noclip and not Settingss.autosea3 and not getgenv().masteryGun and not Settingss.hakikenhop  and not  getgenv().Boss and not atofarmmoball and not atoelite and not atohakikenv2 and not Settingss.atoelitehop and not atoracev2 and not atocitizen and not atolongma and not Settingss.atofarmbounty and not Settingss.atoindra and not atoplayer and not atofarmbossall and not atofarmhakiken and not Settingss.atofully then 
        if plr.Character.HumanoidRootPart:FindFirstChild("EffectsSY") then
            plr.Character.HumanoidRootPart.EffectsSY:Destroy()
        end
    end
end)
