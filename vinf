local autoelitetf = true
local function atoelitehopp()
    for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
    if table.find(mobelitequest,v.Name) then
        getgenv().testelite = v.Name
    end
end
if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true and table.find(mobelite,game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text) then
    local folderl = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild(getgenv().testelite)  then
        folderl = game.ReplicatedStorage
    end
    for i,v in pairs(folderl:GetChildren()) do
        if v.Name == getgenv().testelite and v.Humanoid.Health > 0 then
            pcall(function()
                repeat wait()
                    sizepart(v)
                    tweencomplete(v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                    autoattack()
                    equiptool(Settingss.tool)
                until not v.Parent or not v or game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false or not Settingss.atoelitehop
            end)
        end
    end
    else
        tweencomplete(CFrame.new(-5417.83545, 313.063538, -2823.35913, 0.927179396, 0, 0.374617696, 0, 1, 0, -0.374617696, 0, 0.927179396))
        if (game:GetService("Workspace").NPCs["Elite Hunter"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude < 8 then
            local args = {
                [1] = "EliteHunter"
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
end
end
        
local toggleatoelitehop = MSectionFarmelite.AddToggle("Auto Elite + Hop",{Default_Value = Settingss.atoelitehop},function(v)
    Settingss.atoelitehop = v
    spawn(function()
        while wait() do
            if Settingss.atoelitehop then
                    pcall(function()
                        if autoelitetf == true then
                            if game.ReplicatedStorage:FindFirstChild("Deandre [Lv. 1750]") or game.Workspace.Enemies:FindFirstChild("Deandre [Lv. 1750]") or game.ReplicatedStorage:FindFirstChild("Urban [Lv. 1750]") or game.Workspace.Enemies:FindFirstChild("Urban [Lv. 1750]") or game.ReplicatedStorage:FindFirstChild("Diablo [Lv. 1750]") or game.Workspace.Enemies:FindFirstChild("Diablo [Lv. 1750]") then
                                atoelitehopp()
                                else
                                    wait(3)
                                    pcall(function()
                                    library.AddNoti({Title  ='Auto Elite',Text = 'Hop Server',Button_Text = 'Close', Question = false})
                                    end)
                                    if not  game.ReplicatedStorage:FindFirstChild("Deandre [Lv. 1750]") or not  game.Workspace.Enemies:FindFirstChild("Deandre [Lv. 1750]") or not  game.ReplicatedStorage:FindFirstChild("Urban [Lv. 1750]") or not  game.Workspace.Enemies:FindFirstChild("Urban [Lv. 1750]") or not  game.ReplicatedStorage:FindFirstChild("Diablo [Lv. 1750]") or not game.Workspace.Enemies:FindFirstChild("Diablo [Lv. 1750]") then
                                        spawn(function()
                                             wait(10)
                                             Teleport()
                                        end)
                                        
                                end
                            end
                        end
                    end)
            end
        end
    end)
end)
    
local togglegodchalice = MSectionFarmelite.AddToggle("Stop when God's Chalice",{Default_Value = Settingss.atogodchalice},function(v)
    Settingss.atogodchalice = v
    spawn(function()
        while wait() do
            if Settingss.atogodchalice then
                pcall(function()
                    for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
                        if string.find(v.Name, "rip_indra") then
                            getgenv().autopro = v.Name
                        end
                    end
                    for i,v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if string.find(v.Name, "rip_indra") then
                            getgenv().autoproz = v.Name
                        end
                    end                    
                    if plr.Backpack:FindFirstChild("God's Chalice") or plr.Character:FindFirstChild("God's Chalice") or game.Workspace.Enemies:FindFirstChild(getgenv().autoproz) or game.ReplicatedStorage:FindFirstChild(getgenv().autopro)  then
                        autoelitetf = false
                        else
                            autoelitetf = true
                    end
                end)
            end
        end
    end)
end)
