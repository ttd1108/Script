local FarmPage = a.AddPage('Farm')

local FSectionMics = FarmPage.AddSection("Mics")

local DropWp = FSectionMics.AddDropdown("Select Weapon",{List = Weapon, Search = true, Selected = false, DefaultValue = Settingss.tool}, function(v)
    Settingss.tool = v
end)

local Refreshwp = FSectionMics.AddButton("Refresh Weapons",function()
    for i = 1, #Weapon do
        table.remove(Weapon, 1)
    end
    pcall(function()
        for i, v in pairs(plr.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Gun" or v.ToolTip == "Melee" or v.ToolTip == "Sword" then
                table.insert(Weapon, v.Name)
            end
        end
    
        for i, v in pairs(game.Workspace.Characters[plr.Name]:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Gun" or v.ToolTip == "Melee" or v.ToolTip == "Sword" then
                table.insert(Weapon, v.Name)
            end
        end
    end)
end)


local ToggleBring = FSectionMics.AddToggle("Bring Mob",{Default_Value = Settingss.Bring},function(v)
    Settingss.Bring = v
end)
spawn(function()
    game.RunService.RenderStepped:connect(function()
        if Settingss.Bring then
            pcall(function()
                bring()
            end)
        end 
    end)
end)



