getgenv().ScriptReady = true

local a = library.CreateMain('Vin Hub - Blox Fruit')

local quest = require(game.ReplicatedStorage.Quests)
local toolrac = require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework)

local plr = game.Players.LocalPlayer
local CommF = game.ReplicatedStorage.Remotes["CommF_"]
local RunService = game.RunService
local VirtualUser = game:GetService("VirtualUser")
local Manager = game.VirtualInputManager
local UserInput = game.UserInputService
local Camera = Workspace.Camera.ViewportSize
local HttpService = game.HttpService


local Settingss

if not isfolder('Vin Hub') then
    makefolder('Vin Hub')
end

Settingss = DefaultSettings

---savename
getgenv().DefaultSettingsavename = {
    SaveName = {},
}
local Settingsavename 
if not pcall(function() readfile("Vin Hub/SaveName.json") end) then
    writefile("Vin Hub/SaveName.json", HttpService:JSONEncode(getgenv().DefaultSettingsavename))
end

local sda, xyz = pcall(function()
    Settingsavename = HttpService:JSONDecode(readfile("Vin Hub/SaveName.json"))
end)

if not sda then
    Settingsavename = getgenv().DefaultSettingsavename
end
if not table.find(Settingsavename.SaveName,plr.Name) then
    table.insert(Settingsavename.SaveName,plr.Name)
    writefile("Vin Hub/SaveName.json", HttpService:JSONEncode(Settingsavename))
end
