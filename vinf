function checkkeyhidden()
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v.Name == "Hidden Key" then
			getgenv().namekeyhidden = v.Name
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v.Name == "Hidden Key" then
			getgenv().namekeyhidden = v.Name
		end
	end
	return getgenv().namekeyhidden
end
function autorengoku()
	if game.ReplicatedStorage:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") or game.Workspace.Enemies:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") then
		local folder4 = game.workspace.Enemies
		if game.ReplicatedStorage:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") then
			folder4 = game.ReplicatedStorage
		end
		for i,v in pairs(folder4:GetChildren()) do
			if v.Name == "Awakened Ice Admiral [Lv. 1400] [Boss]" then
				repeat wait()
					autoattack()
					sizepart(v)
                    equiptool(Settingss.tool)
					toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                until not v or not v.Parent or v.Humanoid.Health == 0  or not Settingss.atoRengoku
            end
        end
	else
		if checkkeyhidden() == nil  then
            Teleport()
		elseif checkkeyhidden() ~= nil then
			equiptool(checkkeyhidden())
            toTarget(plr.Character.HumanoidRootPart.Position,Vector3.new(6571.9296875, 298.32537841797, -6966.1860351563),CFrame.new(6571.9296875, 298.32537841797, -6966.1860351563))
		end
    end
end
