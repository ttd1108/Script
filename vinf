                            autoattack()
                            accessory()
                        until not v or not v.Parent or v.Humanoid.Health == 0  or not atolongma
                    end)
                end
            end
        end
end


function autoskillfactory()
    if skilldf['Z'] then
        spawn(function()
        Manager:SendKeyEvent(true, 'Z', false, game)
        wait()
        Manager:SendKeyEvent(false, 'Z', false, game)
        end)
    end

    if skilldf['X'] then
        spawn(function()
        Manager:SendKeyEvent(true, 'X', false, game)
        wait()
        Manager:SendKeyEvent(false, 'X', false, game)
        end)
    end

    if skilldf['C'] then
        spawn(function()
        Manager:SendKeyEvent(true, 'C', false, game)
        wait()
        Manager:SendKeyEvent(false, 'C', false, game)
        end)
    end

    if skilldf['V'] then
        spawn(function()
        Manager:SendKeyEvent(true, 'V', false, game)
        wait()
        Manager:SendKeyEvent(false, 'V', false, game)
        end)
    end

    if skilldf['F'] then
        spawn(function()
        Manager:SendKeyEvent(true, 'F', false, game)
        wait()
        Manager:SendKeyEvent(false, 'F', false, game)
        end)
    end
end

local function factory()
    
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Core") then
        folder = game.ReplicatedStorage
    end

    for i,v in pairs(folder:GetChildren()) do
        if getgenv().AutoCore and v.Name == "Core" and v.Humanoid.Health > 0 then
            repeat wait()
                getgenv().psskill = v.HumanoidRootPart.Position
                autoskillfactory()
                getgenv()
                if getgenv().Farm2 then
                    getgenv().Farm = false
                end
                accessory()
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,CFrame.new(448.46756, 199.356781, -441.389252).Position,CFrame.new(448.46756, 199.356781, -441.389252))
                equiptool(Settingss.tool)
            until not getgenv().AutoCore or v.Humanoid.Health <= 0
            getgenv().psskill = nil
                if getgenv().Farm2 then
                    getgenv().Farm = true
                end   
        end
    end
end

local function autoquestbarto()
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Swan Pirate [Lv. 775]") then
        folder = game.ReplicatedStorage
    end
    if game.Players.LocalPlayer.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 0 then
        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Swan Pirates") and string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "50") and game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
            for i,v in pairs(folder:GetChildren()) do
                if v.Name == "Swan Pirate [Lv. 775]" and v.Humanoid.Health > 0 then
                    pcall(function()
                        repeat wait()
                            sizepart(v)
                            autoattack()
                            accessory()
                            getgenv().bringbarto = v.HumanoidRootPart.CFrame
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                            getgenv().bringbarto1 = true
                        until v.Humanoid.Health == 0 or not getgenv().atobartooo or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false
                        getgenv().bringbarto1 = false
                    end)
                end
            end
        else
            tweencomplete(CFrame.new(-456.28952, 73.0200958, 299.895966))
            local args = {
                [1] = "StartQuest",
                [2] = "BartiloQuest",
                [3] = 1
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    elseif game.Players.LocalPlayer.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 1 then
     local folder1 = game.workspace.Enemies
     if game.ReplicatedStorage:FindFirstChild("Jeremy [Lv. 850] [Boss]") then
         folder1 = game.ReplicatedStorage
     end
     for i,v in pairs(folder1:GetChildren()) do
         if v.Name == "Jeremy [Lv. 850] [Boss]" and v.Humanoid.health > 0 then
             pcall(function()
                 repeat wait()
                    autoattack()
                     accessory()
                     equiptool(Settingss.tool)
                     sizepart(v)
                     toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                 until v.Humanoid.Health == 0 or not getgenv().atobartooo or not v.Parent
             end)
         end
     end
    elseif game.Players.LocalPlayer.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 2 then
        tweencomplete(CFrame.new(-1850.49329, 13.1789551, 1750.89685))
        wait(2)
        tweencomplete(CFrame.new(-1858.87305, 19.3777466, 1712.01807))
        wait(2)
        tweencomplete(CFrame.new(-1803.94324, 16.5789185, 1750.89685))
        wait(2)
        tweencomplete(CFrame.new(-1858.55835, 16.8604317, 1724.79541))
        wait(2)
        tweencomplete(CFrame.new(-1869.54224, 15.987854, 1681.00659))
        wait(2)
        tweencomplete(CFrame.new(-1800.0979, 16.4978027, 1684.52368))
        wait(2)
        tweencomplete(CFrame.new(-1819.26343, 14.795166, 1717.90625))
        wait(2)
        tweencomplete(CFrame.new(-1813.51843, 14.8604736, 1724.79541))
        wait(2)
    end
end

local function bringbartoo()
pcall(function()
    for i, v1 in pairs(game.workspace.Enemies:GetChildren()) do
        if v1.Name == "Swan Pirate [Lv. 775]" then
            if v1:FindFirstChild("Humanoid") and v1.Humanoid.Health ~= 0 and isnetworkowner(v1.HumanoidRootPart) then
                sizepart(v1)
                v1.HumanoidRootPart.CFrame = getgenv().bringbarto
                autoattack()
                
                equiptool(Settingss.tool)
            end
        end
    end
end)
end


local function upracev2()
if   not plr.Backpack:FindFirstChild("Flower 1") and not  plr.Backpack:FindFirstChild("Flower 2") and  not plr.Backpack:FindFirstChild("Flower 3")  then
    tweencomplete(CFrame.new(-2774.83, 73.8141, -3568.67))
    if (game:GetService("Workspace").NPCs.Alchemist.HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
        local args = {
            [1] = "Alchemist",
            [2] = "2"
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    end
    elseif  plr.Backpack:FindFirstChild("Flower 1") and   plr.Backpack:FindFirstChild("Flower 2") and  plr.Backpack:FindFirstChild("Flower 3") then
    tweencomplete(CFrame.new(-2774.83, 73.8141, -3568.67))
    if (game:GetService("Workspace").NPCs.Alchemist.HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
        local args = {
            [1] = "Alchemist",
            [2] = "3"
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
end
        
if not plr.Backpack:FindFirstChild("Flower 1")   then
    tweencomplete(game:GetService("Workspace").Flower1.CFrame)
elseif not plr.Backpack:FindFirstChild("Flower 2")  then
    tweencomplete(game:GetService("Workspace").Flower2.CFrame)
elseif not plr.Backpack:FindFirstChild("Flower 3")  then
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Marine Captain [Lv. 900]") then
        folder = game.ReplicatedStorage
    end
    
for i,v in pairs(folder:GetChildren()) do
    if v.Name == "Marine Captain [Lv. 900]" and v.Humanoid.Health > 0 then
        getgenv().bringtocv2 = v.HumanoidRootPart.CFrame
        repeat wait() 
            autoattack()
            equiptool(Settingss.tool)
            sizepart(v)
            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
        until not v or not v.Parent or v.Humanoid.Health == 0 or plr.Backpack:FindFirstChild("Flower 3") or  plr.Character:FindFirstChild("Flower 3") or atoracev2
        end
    end
end
end

