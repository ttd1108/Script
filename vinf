testttt = game.Players.LocalPlayer.Name
local farmplayer = a.AddPage("Kill Player")

local FSectionplayer = farmplayer.AddSection("Kill Player")
PlayerName = {}
for i, v in pairs(game.Players:GetChildren()) do
    if not string.find(v.Name, testttt) then
        table.insert(PlayerName, v.Name)
    end
end

for i, v in pairs(game.Players:GetChildren()) do
    if v:IsA("Tool") then
        table.insert(PlayerName, v.Name)
    end
end
game.Players.ChildAdded:Connect(function()
    for i = 1, #PlayerName do
        table.remove(PlayerName, 1)
    end
    for i, v in ipairs(game.Players:GetChildren()) do
        if not string.find(v.Name, testttt) then
            table.insert(PlayerName, v.Name)
        end
    end
end)

game.Players.ChildRemoved:Connect(function()
    for i = 1, #PlayerName do
        table.remove(PlayerName, 1)
    end
    for i, v in ipairs(game.Players:GetChildren()) do
        if not string.find(v.Name, testttt) then
            table.insert(PlayerName, v.Name)
        end
    end
end)

local selectplayer = FSectionplayer.AddDropdown("Select Player", {List = PlayerName, Search = true, Selected = false}, function(v)
    selecplayer = v
end)
local distanceplayerr = FSectionplayer.AddSlider('Distance',{Min = 0, Max = 200, Default_Value = 50, Precise = false},function(v)
    getgenv().distanceplayer = v
end)
local function attackPlayer()
    spawn(function()
    if toolrac.activeController and toolrac.activeController.anims and toolrac.activeController.anims.basic then 
        toolrac.activeController.attacking = false
        toolrac.activeController.increment = 1
        toolrac.activeController.timeToNextAttack = 0
        toolrac.activeController.hitboxMagnitude=100
        toolrac.activeController.blades = {game.Players.LocalPlayer.Character.HumanoidRootPart}
    end
    end)
    spawn(function()
    game:GetService("VirtualUser"):CaptureController()
    game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
    end)
end


local toggleautoplayer = FSectionplayer.AddToggle("Auto Player",{Default_Value = false},function(v)
    atoplayer = v
    spawn(function()
        while wait() do
            if atoplayer then
                pcall(function()
                    for i,v in pairs(game.Workspace.Characters:GetChildren()) do
                        if v.Name == selecplayer and v.Humanoid.Health > 0 then
                            pcall(function()
                                repeat wait()
                                    attackPlayer()
                                    plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, getgenv().distanceplayer, 0)
                                until not v or not v.Parent or v.Humanoid.Health == 0 or  not atoplayer
                            end)
                        end
                    end
                end)
            end
        end
    end)
end)

function ClosestPartaimbot()
    local dist = math.huge
    getgenv().targetaimbot = nil
    for i,v in pairs(game.Workspace.Characters:GetChildren()) do
        if v:IsA("Model")  then
            if v.Name ~= plr.Name then
                local conconcac = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.HumanoidRootPart.Position).Magnitude
                if conconcac < dist then
                    dist = conconcac
                    getgenv().targetaimbot = v.Name
                end
            end
        end
    end
    return getgenv().targetaimbot
end

getgenv().toggleaimbot = FSectionplayer.AddToggle("Aimbot",{Default_Value = false},function(v)
    atoaimbot = v
    spawn(function()
        while wait() do
            if atoaimbot then
                for i,v in pairs(game.Workspace.Characters:GetChildren()) do
                    if v:IsA("Model") then
                        if  v.Name == ClosestPartaimbot() and v.Humanoid.Health > 0 then
                            getgenv().psskill = v.HumanoidRootPart.Position
                        end
                    end
                end
            end
        end
    end)
end)


getgenv().cailonmemay = 0
getgenv().concacbagiamay = 0
local saveplayer = {}
function onlygun(v)
    spawn(function()
        pcall(function()
            local args = {
                [1] = v.HumanoidRootPart.Position,
                [2] = v.HumanoidRootPart
            }   
            game:GetService("Players").LocalPlayer.Character[getgenv().checkgunn].RemoteFunctionShoot:InvokeServer(unpack(args))
        end)
    end)
end
function checktimefarmbounty(v)
    spawn(function()
        if v.Humanoid.Health / v.Humanoid.MaxHealth <= 90/100 then
            if getgenv().concacbagiamay >= 200 then
                getgenv().concacbagiamay = 0
                getgenv().checktimefarmbountyyyy = true
            end
            if getgenv().concacbagiamay < 200 then
                wait(1)
                getgenv().checktimefarmbountyyyy = false
                getgenv().concacbagiamay = getgenv().concacbagiamay + 1
            end
        else
            checksafezone(v)
            if getgenv().cailonmemay < 450 then
                wait(1)
                getgenv().checktimefarmbountyyyy = false
                getgenv().cailonmemay = getgenv().cailonmemay + 1
            end
            if getgenv().cailonmemay >= 450 then
                getgenv().cailonmemay = 0
                getgenv().checktimefarmbountyyyy = true
            end
        end
    end)
end

function checktimbountycc()
    return getgenv().checktimefarmbountyyyy
end

function checkconcaccailonmemay(v)
    spawn(function()
        pcall(function()
            local args = {
                [1] = v.HumanoidRootPart.Position,
                [2] = v.HumanoidRootPart
            }   
            game:GetService("Players").LocalPlayer.Character[getgenv().checkgunn].RemoteFunctionShoot:InvokeServer(unpack(args))
        end)
    end)
    spawn(function()
        if game.Players.LocalPlayer.PlayerGui.Main.PvpDisabled.Visible == true then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EnablePvp")
        end
    end)
end

function equipgun()
    for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
        if v:IsA("Tool") and v:FindFirstChild("RemoteFunctionShoot")  then
            v.Parent = plr.Character
            getgenv().checkgunn = v.Name
        end
        
    end
end
countconcacditbagiamay = 0
countautoattackbounty = 0
function autoattackbounty(v)
    countautoattackbounty = countautoattackbounty +1
    if countautoattackbounty == 0 and v.Humanoid.Health / v.Humanoid.MaxHealth >= 20/100 then
        autoattack()
    elseif countautoattackbounty == 20 and v.Humanoid.Health / v.Humanoid.MaxHealth >= 20/100 then
        autoattack()
    elseif countautoattackbounty >= 70  then
        countautoattackbounty = 0
    end
end

function autoattackplayer()
    pcall(function()
    if toolrac.activeController and toolrac.activeController.anims and toolrac.activeController.anims.basic then                    
        toolrac.activeController.hitboxMagnitude= 10       
        toolrac.activeController.blades = {game.Players.LocalPlayer.Character.HumanoidRootPart}
        toolrac.activeController:attack()                                                    
    end
end)
end



function autoskillpro(v1)
    spawn(function()
        for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetChildren()) do
            if v:IsA("RemoteFunction") and plr.Character.Humanoid.Health > 0 and (plr.Character.HumanoidRootPart.Position-v1.HumanoidRootPart.Position).Magnitude < 20 then
                pcall(function()
                    spawn(function()
                        v:InvokeServer("Z")
                        getgenv().psskill = v1.HumanoidRootPart.Position
                    end)
                    spawn(function()
                        v:InvokeServer("X")
                        getgenv().psskill = v1.HumanoidRootPart.Position
                    end)
                    spawn(function()
                        pcall(function()
                            v:InvokeServer("C")
                            getgenv().psskill = v1.HumanoidRootPart.Position
                        end)
                    end)
                end)
            end
        end
    end)
end

local function checkraidbounty(p)
    for i,v in pairs(game.Workspace["_WorldOrigin"].Locations:GetChildren()) do
        if string.find(v.Name, "Island") then
            if game.Workspace["_WorldOrigin"].Locations:FindFirstChild(v.Name) then
                if (v.Position-p.HumanoidRootPart.Position).Magnitude <= 1000 then
                    getgenv().Checkraids = true 
                    return true
                end
            end
        end
    end
    return false
end

function checksafezone(p)
    for i,v in pairs(game:GetService("Workspace")["_WorldOrigin"].SafeZones:GetChildren()) do
        if v:IsA("Part") then
            if (v.Position-p.HumanoidRootPart.Position).magnitude <= 400 and p.Humanoid.Health / p.Humanoid.MaxHealth >= 90/100 then
                getgenv().CheckSafeZone = true
                return true
            end
        end
    end
    return false
end

function checkfarmbountyhop()
    getgenv().yeayea = {}
    for i,v in pairs(game:GetService("Workspace").Characters:GetChildren()) do
        if not table.find(saveplayer,v.Name)  and not string.find(v.Name, testttt) and (game.Players.LocalPlayer.Team == game.Teams.Marines and game.Players[v.Name].Team ~= game.Teams.Marines or game.Players.LocalPlayer.Team ~= game.Teams.Marines) then
            table.insert(getgenv().yeayea,v.Name)
        end
    end
    return getgenv().yeayea
end

function ClosestPart()
    if getgenv().target == nil then
        local dist = math.huge
        getgenv().target = nil
        for i,v in pairs(game:GetService("Workspace").Characters:GetChildren()) do
            if v.ClassName == "Model" then
                if  table.find(checkfarmbountyhop(),v.Name)  and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0  then
                    local magnitude = (v.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).magnitude
                    if magnitude < dist then
                        dist = magnitude
                        getgenv().target = v.Name
                    end
                end
            end
        end
    end
    return getgenv().target
end
function teleportdie(v)
    if (v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 7000 and (v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 20000 then
        if game:GetService("Players").LocalPlayer.PlayerGui.Main.InCombat.Text ~= "In Combat - Bounty at risk!" or game:GetService("Players").LocalPlayer.PlayerGui.Main.InCombat.Visible == false then
            plr.Character.Humanoid.Health = 0
            wait(0.1)
            plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
            tweenmain:Pause()
            tweenmain:Cancel()
            getgenv().timee = tick()
            getgenv().cailonmemay = 0
            getgenv().concacbagiamay = 0
            repeat wait()
            until tick()-getgenv().timee >= 0.6
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetSpawnPoint")
            tweenmain:Pause()
            tweenmain:Cancel()
        end
    end
end
function aimbotskillfarmbounty(v)
    spawn(function()
        getgenv().psskill = v.HumanoidRootPart.Position
    end)
end

function sendfarmbounty(url,title,v)
    msg = {
        ["content"] = "",
        ["embeds"] = {
            {
            ["color"] = tonumber(0x000000),
            ["title"] =  title.."- Vin Hub",
            ["fields"] = {
                {
                ["name"] = "Name Target :",
                ["value"] = "||"..v.."||",
                ["inline"] = false
            },
            {
            ["name"] = "Bounty/Honor :",
            ["value"] = game.Players[v].leaderstats["Bounty/Honor"].Value,
            ["inline"] = false,  
            }
            },
            }
        }
    } 
    local request = syn.request or http_request
    local response = request(
    {
    Url = url,
    Method = "POST",
    Headers = {
    ["Content-Type"] = "application/json"
    },
    Body = game:GetService("HttpService"):JSONEncode(msg)
    }
    )
end
conbagiamay = 0
conbagiamaycc = 0
function webhookfarmbounty(v)
    spawn(function()
        if conbagiamay == 0 and Settingss.atowebhookfarmbounty then
            conbagiamay = conbagiamay + 1
            sendfarmbounty(Settingss.urlwebhooklevel,"Hunt Bounty",v)
        end
    end)
end
function webhookprofilebounty()
    if conbagiamaycc == 0 and Settingss.atowebhookfarmbounty then
        conbagiamaycc = conbagiamaycc +1
        sendfarmbounty(Settingss.urlwebhooklevel,"Bounty Profile",plr.Name)
    end
end



local TargetPlayer = FSectionplayer.AddLabel("Name Target :")
local HealthPlayer = FSectionplayer.AddLabel("Health Player :")
function vodungconcac(v)
    spawn(function()
        TargetPlayer.SetText("Name Target :"..v.Name.."("..(v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude..")")
        HealthPlayer.SetText("Health Player :"..v.Humanoid.Health)
        tweenmain:Pause()
        tweenmain:Cancel()
    end)
end

getgenv().targetgodmode = nil
function ClosestParthakiken()
    local dist = math.huge
    if getgenv().targetgodmode == nil then
        for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
            if v:IsA("Model")  then
                if  string.find(v.Name, 'Lv') and v:FindFirstChild("HumanoidRootPart") then
                    local conconcac = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.HumanoidRootPart.Position).Magnitude
                    if conconcac < dist then
                        dist = conconcac
                        getgenv().targetgodmode = v
                    end
                end
            end
        end
    end
    return getgenv().targetgodmode
end
concacgido = 0
function godmode()
    AutoBuso()
    if plr.Character.Humanoid.MaxHealth == 100 and game.Players.LocalPlayer.PlayerGui.Main.HP.TextLabel.Text == "Health 100/100" and plr.Character.Humanoid.Health > 0 then
        if not game:FindFirstChild("1") and plr.Character:FindFirstChild("HasBuso") and game.Players.LocalPlayer.PlayerGui.Main.HP.TextLabel.Text == "Health 100/100" and plr.Character.Humanoid.MaxHealth == 100  then
            game.Players.LocalPlayer.Character.Humanoid:SetStateEnabled(15,false)
            game.Players.LocalPlayer.Character.Humanoid.Name = 1
            local l = game.Players.LocalPlayer.Character["1"]:Clone()
            l.Parent = game.Players.LocalPlayer.Character
            l.Name = "Humanoid"
            game.Players.LocalPlayer.Character["1"].Parent = game
            game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
        elseif game:FindFirstChild("1") and game:FindFirstChild("1").Health > 0 and plr.Character:FindFirstChild("HasBuso") and game.Players.LocalPlayer.PlayerGui.Main.HP.TextLabel.Text == "Health 100/100"   then
            toTarget(plr.Character.HumanoidRootPart.Position,ClosestParthakiken().HumanoidRootPart.Position,ClosestParthakiken().HumanoidRootPart.CFrame*CFrame.new(0,0,2))
        end
    end
    if  game.Players.LocalPlayer.PlayerGui.Main.HP.TextLabel.Text == "Health 0/100"   then
        game.Players.LocalPlayer.Character.Humanoid.Health = Die
        if game:FindFirstChild("1") then 
            game:FindFirstChild("1"):Destroy()
        end
    end
    if plr.Character.Humanoid.MaxHealth ~= 100  then
        library.AddNoti({Title  ='Farm Bounty',Text = 'request Health 100/100 and Stats Defense = 1, unequip accessory ',Button_Text = 'Close', Question = false})
    end
end
