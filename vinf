local whitelistedfruit = {
    "Quake Fruit",
    "Human: Buddha Fruit",
    "String Fruit",
    "Bird: Phoenix Fruit",
    "Rumble Fruit",
    "Paw Fruit",
    "Gravity Fruit",
    "Dough Fruit",
    "Shadow Fruit",
    "Venom Fruit",
    "Control Fruit",
    "Dragon Fruit"
}




cframethangdaubuoiredhead = CFrame.new(-1926.78772, 12.1678171, 1739.80884, 0.956294656, -0, -0.292404652, 0, 1, -0, 0.292404652, 0, 0.956294656)
function getbackpack()
    mybackpack = {}
    for i,v in next, game.Players.LocalPlayer.Backpack:GetChildren() do
        if v:IsA("Tool") and table.find(whitelistedfruit,v.Name) then
            table.insert(mybackpack,v.Name)
        end
    end
    for i1,v1 in next, game.Players.LocalPlayer.Character:GetChildren() do
        if v1:IsA("Tool") and table.find(whitelistedfruit,v1.Name) then
            table.insert(mybackpack,v1.Name)
        end
    end
    return mybackpack
end
getgenv().tablefruitausea3 = {
    ["1000000"] ="Quake-Quake",
    ["1200000"] = "Human-Human: Buddha",
    ["1500000"] = "String-String",
    ["1800000"] = "Bird-Bird: Phoenix",
    ["2100000"] = "Rumble-Rumble",
    ["2300000"] = "Paw-Paw",
    ["2500000"] = "Gravity-Gravity",
    ["2800000"] = "Dough-Dough",
    ["2900000"] = "Shadow-Shadow",
    ["3000000"] = "Venom-Venom",
    ["3200000"] = "Control-Control",
    ["3500000"] = "Dragon-Dragon"
}
function getfruitstore()
    local max = math.huge
    local name 
    for i, v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventoryFruits")) do
        for i1,v1 in pairs(v) do
            if i1 == "Name" then
                for i2, v2 in pairs(getgenv().tablefruitausea3) do
                    if v1 == v2 then
                        if tonumber(i2) < tonumber(max) then
                            max = i2
                            name = v2
                            --print(v2)
                            --game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LoadFruit",v1)
                        end
                    end
                end
            end
        end
    end
    return name
end

local togglebartobring = MSectionFarm.AddToggle("Auto Third Sea",{Default_Value = Settingss.autosea3},function(v)
    Settingss.autosea3 = v
    spawn(function()
        while wait() do
            if Settingss.autosea3 then
                pcall(function()
                    if plr.Data.Level.Value >= 1500 then                                                                                
                        if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("TalkTrevor", "1") ~= 0 then
                            if checkdevilfruit() == nil and Settingss.togglegetfruit then
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LoadFruit",getfruitstore())
                            end
                            if #getbackpack() >= 1 and game.ReplicatedStorage.Remotes.CommF_:InvokeServer("TalkTrevor", "1") ~= 0 then
                                if getgenv().Farm2 then
                                    tweenmain:Pause()
                                    tweenmain:Cancel()
                                    getgenv().Farm = false
                                end
                                wait(2)
                                toTarget(plr.Character.HumanoidRootPart.Position,Vector3.new(-339.79840087891, 331.86065673828, 643.83178710938),CFrame.new(-339.79840087891, 331.86065673828, 643.83178710938))
                                if (Vector3.new(-339.79840087891, 331.86065673828, 643.83178710938)-plr.Character.HumanoidRootPart.Position).Magnitude <= 5 then
                                    if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("TalkTrevor", "1") ~= 1 then
                                        local ohString1 = "TalkTrevor"
                                        local ohString2 = "1"
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1, ohString2)
                                        local ohString1 = "TalkTrevor"
                                        local ohString2 = "2"
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1, ohString2)
                                    end
                                    if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("TalkTrevor", "1") >= 1 then
                                        for i,v in next, getbackpack() do
                                            plr.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild(v))
                                        end
                                        local ohString1 = "TalkTrevor"
                                        local ohString2 = "1"
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1, ohString2)
                                        wait()
                                        local ohString1 = "TalkTrevor"
                                        local ohString2 = "3"
    
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1, ohString2)
                                    end
                                end
                            elseif #getbackpack() == 0 and game.ReplicatedStorage.Remotes.CommF_:InvokeServer("TalkTrevor", "1") ~= 0 and plr.Data.Level.Value >= 1500 then
                            end
                        else
                            if getgenv().Farm2 then
                                getgenv().Farm = false
                            end
                            if plr.Data.Level.Value >= 1500 then
                                if tweenmain ~= nil then
                                    tweenmain:Pause()
                                    tweenmain:Cancel()
                                end
                                if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ZQuestProgress", "Check") ~= 0 then
                                    if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ZQuestProgress", "Check") == 1 then
                                        if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ZQuestProgress", "Zou") == 0 then
                                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou")
                                        end
                                    elseif game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ZQuestProgress", "Check") == nil then
                                        if getgenv().Farm2 then
                                            tweenmain:Pause()
                                            tweenmain:Cancel()
                                            getgenv().Farm = false
                                        end
                                        if not workspace.Enemies:FindFirstChild("Don Swan [Lv. 1000] [Boss]") then
                                            local ohString1 = "GetUnlockables"
                                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1)
                                            local ohString1 = "requestEntrance"
                                            local ohVector32 = Vector3.new(2284.912109375, 15.537666320801, 905.48291015625)
                                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1, ohVector32)
                                        elseif workspace.Enemies:FindFirstChild("Don Swan [Lv. 1000] [Boss]") then
                                            for i,v in next, workspace.Enemies:GetChildren() do
                                                if v.Name == "Don Swan [Lv. 1000] [Boss]" and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v:IsA("Model") and v.Humanoid.Health > 0 then
                                                    if (v.HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 300 then
                                                        toTarget(plr.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame)
                                                    else
                                                        if tweenmain ~= nil then
                                                            tweenmain:Pause()
                                                            tweenmain:Cancel()
                                                        end
                                                        repeat wait()
                                                            plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,30,0)
                                                            autoattack()
                                                            equiptool(Settingss.tool)
                                                        until v.Humanoid.Health <= 0 or not v
                                                    end
                                                end
                                            end
                                        end
                                    end
                                else
                                    if tweenmain ~= nil then
                                        tweenmain:Pause()
                                        tweenmain:Cancel()
                                    end
                                    if getgenv().Farm2 then
                                        getgenv().Farm = false
                                    end
                                    if (plr.Character.HumanoidRootPart.Position-game:GetService("Workspace").Map.IndraIsland.Part.Position).Magnitude > 1000 then
                                        toTarget(plr.Character.HumanoidRootPart.Position,cframethangdaubuoiredhead.p,cframethangdaubuoiredhead)
                                        if (cframethangdaubuoiredhead.p-plr.Character.HumanoidRootPart.Position).Magnitude <= 5 then
                                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ZQuestProgress","Begin")
                                        end
                                    else
                                        if getgenv().Farm2 then
                                            getgenv().Farm = false
                                        end
                                        for i,v in next, workspace.Enemies:GetChildren() do
                                            if v.Name == "rip_indra [Lv. 1500] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                                if (v.HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 300 then
                                                    toTarget(plr.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame)
                                                else
                                                    repeat wait()
                                                        equiptool(Settingss.tool)
                                                        autoattack()
                                                        plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,30,0)
                                                    until not workspace.Enemies:FindFirstChild("rip_indra [Lv. 1500] [Boss]")
                                                end
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end
    end)
end)
end

if game.PlaceId == 2753915549 then
    local function getkey()
    if (CFrame.new(4852.2895507813, 5.651451587677, 718.53070068359).Position - plr.Character.HumanoidRootPart.Position).magnitude < 5 then
        game.ReplicatedStorage.Remotes["CommF_"]:InvokeServer("DressrosaQuestProgress", "Detective")
        equiptool("Key")
    else
        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(4852.2895507813, 5.651451587677, 718.53070068359),CFrame.new(4852.2895507813, 5.651451587677, 718.53070068359))
    end
end
local ToggleNewWorld = MSectionFarm.AddToggle("Auto New World", {Default_Value = Settingss.autonewworld}, function(v)
    Settingss.autonewworld = v
    spawn(function()
        while Settingss.autonewworld and wait() do
            pcall(function()
                if plr.Data.Level.Value >= 700 and game.Workspace.Map.Ice.Door.CanCollide then
                    getgenv().Farm = false
                    waitdie()
                    if not plr.Character:FindFirstChild("Key") and not plr.Backpack:FindFirstChild("Key") then 
                        getkey()
                        if plr.Character:FindFirstChild("Key") then
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game.Workspace.Map.Ice.Door.Position,game.Workspace.Map.Ice.Door.CFrame)
                        end
                    else
                        equiptool("Key")
                    end
                else
                    if plr.Data.Level.Value >= 700 then
                        local folder = game.workspace.Enemies
                        if game.ReplicatedStorage:FindFirstChild("Ice Admiral [Lv. 700] [Boss]") then
                        folder = game.ReplicatedStorage
                    end
                    for i,v in pairs(folder:GetChildren()) do
                        if v.Name == "Ice Admiral [Lv. 700] [Boss]" then
                            if v:FindFirstChild("Humanoid") and v.Humanoid.Health ~= 0 then
                                repeat wait()
                                    AutoBuso()
                                    sizepart(v)
                                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                                    autoattack()
                                    accessory()
                                until v.Humanoid.Health <= 0 or not Settingss.autonewworld
                                wait(1)
                                local args = {
                                    [1] = "TravelDressrosa"
                                }
                                game.ReplicatedStorage.Remotes["CommF_"]:InvokeServer(unpack(args))
                            end
                        end
                    end
                end
            end
        end)
    end
    end)
end)
local buttons = {
    [1] = CFrame.new(-1180.60046, 21.9781036, 188.301041, 1, 0, 0, 0, 1, 0, 0, 0, 1),
    [2] = CFrame.new(-1421.0018310547, 48.377368927002, 22.514221191406), -- button2,
    [3] = CFrame.new(-1648.6013183594, 23.377464294434, 439.15209960938), -- button3
    [4] = CFrame.new(-1151.4967041016, 2.6681258678436, -700.48193359375), -- button4
    [5] = CFrame.new(-1325.29138, 28.7468262, -461.524445, 1, 0, 0, 0, 1, 0, 0, 0, 1) -- button5
}
function autosaberhaha()
    if plr.Data.Level.Value >= 200 then
        if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").UsedRelic == false then
            if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ProQuestProgress", "SickMan") ~= 0 then
                for i,v in next, game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").Plates do
                    if v == false then
                        tweencomplete(buttons[i])
                    end
                end
                if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").UsedTorch == false then
                    repeat wait()
                        tweencomplete(CFrame.new(-1610.2531738281, 12.177388191223, 162.61180114746))
                    until plr.Backpack:FindFirstChild("Torch") or plr.Character:FindFirstChild("Torch")
                    plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild("Torch"))
                    repeat wait()
                        tweencomplete(CFrame.new(1116.3449707031, 5.0467972755432, 4348.869140625))
                    until not plr.Backpack:FindFirstChild("Torch") and not plr.Character:FindFirstChild("Torch")
                    repeat wait()
                        tweencomplete(CFrame.new(1114.0466308594, 8.082200050354, 4365.7836914063))
                    until plr.Backpack:FindFirstChild("Cup") or plr.Character:FindFirstChild("Cup")
                end
                if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").UsedCup == false and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").UsedTorch == true then
                    if not plr.Backpack:FindFirstChild("Cup") and not plr.Character:FindFirstChild("Cup") then
                        repeat wait()
                            tweencomplete(CFrame.new(1114.0466308594, 8.082200050354, 4365.7836914063))
                        until plr.Backpack:FindFirstChild("Cup") or plr.Character:FindFirstChild("Cup")
                    end
                    repeat wait()
                        plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild("Cup"))
                        tweencomplete(CFrame.new(1395.1201171875, 37.473316192627, -1324.3107910156))
                    until plr.Backpack:FindFirstChild("Cup").Handle.BrickColor == BrickColor.new("Teal") or plr.Character:FindFirstChild("Cup").Handle.BrickColor == BrickColor.new("Teal")
                    tweencomplete(CFrame.new(1457.8768310547, 88.377502441406, -1390.6892089844))
                    local ohString1 = "ProQuestProgress"
                    local ohString2 = "SickMan"

                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(ohString1, ohString2)
                end
            end
            if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ProQuestProgress", "RichSon") == 0 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").KilledMob == false then
                if game.ReplicatedStorage:FindFirstChild("Mob Leader [Lv. 120] [Boss]") then
                    for i,v in next, game.ReplicatedStorage:GetChildren() do
                        if v.Name == "Mob Leader [Lv. 120] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            tweencomplete(v.HumanoidRootPart.CFrame*CFrame.new(0,0,-50))
                        end
                    end
                elseif workspace.Enemies:FindFirstChild("Mob Leader [Lv. 120] [Boss]") then
                    for i1,v1 in next, workspace.Enemies:GetChildren() do
                        if v1.Name == "Mob Leader [Lv. 120] [Boss]" and v1:FindFirstChild("HumanoidRootPart") and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0 then
                            sizepart(v1)
                            plr.Character.HumanoidRootPart.CFrame = v1.HumanoidRootPart.CFrame * GetPosPlus(v1.HumanoidRootPart.CFrame)
                            autoattack()
                        end
                    end
                end
            elseif game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ProQuestProgress", "RichSon") == 1 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress").UsedRelic == false then
            if not plr.Character:FindFirstChild("Relic") and not plr.Backpack:FindFirstChild("Relic") then
                tweencomplete(CFrame.new(-908.12579345703, 13.877366065979, 4077.5068359375))
                game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ProQuestProgress", "RichSon")
            end
            if plr.Character:FindFirstChild("Relic") or plr.Backpack:FindFirstChild("Relic") then
                tweencomplete(CFrame.new(-1405.3677978516, 29.977333068848, 4.5685839653015))
                plr.Character.Humanoid:EquipTool(plr.Backpack:FindFirstChild("Relic"))
            end
        else
            if game.ReplicatedStorage:FindFirstChild("Saber Expert [Lv. 200] [Boss]") then
                for i,v in next, game.ReplicatedStorage:GetChildren() do
                    if v.Name == "Saber Expert [Lv. 200] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        tweencomplete(v.HumanoidRootPart.CFrame*CFrame.new(0,0,-50))
                    end
                end
            end
            for i,v in next, workspace.Enemies:GetChildren() do
                if v.Name == "Saber Expert [Lv. 200] [Boss]" and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                    repeat wait()
                        sizepart(v)
                        plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * GetPosPlus(v.HumanoidRootPart.CFrame)
                        autoattack()
                    until v.Humanoid.Health <= 0 or not atosaber
                end
            end
        end
    end
end
end
    MSectionFarm.AddToggle("Auto Saber",{Default_Value=false},function(v)
        atosaber = v 
    end)
    spawn(function()
        while wait() do
            if atosaber then
                pcall(function()
                    autosaberhaha()
                end)
            end
        end
    end)
    getgenv().Atopole = MSectionFarm.AddToggle("Auto Pole+ Hop",{Default_Value = Settingss.atopolehop},function(v)
        Settingss.atopolehop = v 
        spawn(function()
            while wait() do
                if Settingss.atopolehop then
                    pcall(function()
                        if game.ReplicatedStorage:FindFirstChild("Thunder God [Lv. 575] [Boss]") or game.Workspace.Enemies:FindFirstChild("Thunder God [Lv. 575] [Boss]") then
                            local folder4 = game.workspace.Enemies
                            if game.ReplicatedStorage:FindFirstChild("Thunder God [Lv. 575] [Boss]") then
                                folder4 = game.ReplicatedStorage
                            end
                            for i,v in pairs(folder4:GetChildren()) do
                                if v.Name == "Thunder God [Lv. 575] [Boss]" then
                                    repeat wait()
                                        autoattack()
                                        sizepart(v)
                                        equiptool(Settingss.tool)
                                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                                    until not v or not v.Parent or v.Humanoid.Health == 0  or not Settingss.atopolehop
                                end
                            end
                        else
                            Teleport()
                        end
                    end)
                end
            end
        end)
    end)
end

