local function TeleportElite()
    wait(1)
    TPReturner()
    if foundAnything ~= "" then
        wait(3)
        TPReturner()
    end
end
getgenv().targethakiken = nil
function ClosestParthakikenmn()
    local dist = math.huge
    if getgenv().targethakiken == nil then
        for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
            if v:IsA("Model")  then
                if  string.find(v.Name, 'Lv') and v:FindFirstChild("HumanoidRootPart") then
                    local conconcac = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position-v.HumanoidRootPart.Position).Magnitude
                    if conconcac < dist then
                        dist = conconcac
                        getgenv().targethakiken = v.Name
                    end
                end
            end
        end
    end
    return getgenv().targethakiken
end

local function autofarmhakiken()
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild(ClosestParthakikenmn()) then
        folder = game.ReplicatedStorage
    end
    if game.Players.LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("ImageLabel") then
        for i,v in pairs(folder:GetChildren()) do
            if v.Name == ClosestParthakikenmn() and v.Humanoid.Health > 0 and game.Players.LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("ImageLabel") then
                pcall(function()
                    repeat wait()
                        getgenv().psfarmhakiken = v.HumanoidRootPart.CFrame
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame+Vector3.new(2,0,0))
                    until  not Settingss.atofarmhakiken or  not v or not v.Parent or not game.Players.LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("ImageLabel")
                    repeat wait()
                        if not game.Players.LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("ImageLabel") then
                            game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game)
                            game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, game)
                        end
                        plr.Character.HumanoidRootPart.CFrame = getgenv().psfarmhakiken*CFrame.new(0,20,0)
                        if Settingss.hakikenhop then
                            library.AddNoti({Title  ='Auto Observation',Text = 'Hop Server',Button_Text = 'Close', Question = false})
                            print("hop")
                            TeleportElite()
                        end
                    until game.Players.LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("ImageLabel")
                end)
            end
        end
    else
        game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game)
        game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, game)
    end
end
local toggleFarmhakiken = MSectionFarm.AddToggle("Farm Observation",{Default_Value = Settingss.atofarmhakiken},function(v)
    Settingss.atofarmhakiken = v
    spawn(function()
        while wait() do
            if Settingss.atofarmhakiken then
                pcall(function()
                    autofarmhakiken()
                end)
            end
        end
    end)
end)

