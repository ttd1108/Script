function sendmsg(url,Sword,valueCheck,ValueCheck2)
    msg = {
        ["content"] = "@everyone",
        ["embeds"] = {
            {
            ["color"] = tonumber(0x000000),
            ["title"] = ValueCheck2.."- Vin Hub",
            ["fields"] = {
                {
                ["name"] = "Name :",
                ["value"] = "||"..game.Players.LocalPlayer.Name.."||",
                ["inline"] = false
            },
            {
            ["name"] = valueCheck,
            ["value"] = Sword,
            ["inline"] = false,  
            }
            },
            }
        }
    } 
    local request = syn.request or http_request
    local response = request(
    {
    Url = url,
    Method = "POST",
    Headers = {
    ["Content-Type"] = "application/json"
    },
    Body = game:GetService("HttpService"):JSONEncode(msg)
    }
    )
end


function checkwpmelee()
    getgenv().namewpmelee = "Nil"
    getgenv().masterywpmelee = "Nil"
    wait()
    pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Melee" then
                getgenv().namewpmelee = v.Name
                getgenv().masterywpmelee = v.Level.Value
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Melee" then
                getgenv().namewpmelee = v.Name
                getgenv().masterywpmelee = v.Level.Value
            end
        end
    end)
end
function checkwpsword()
    getgenv().namewpsword = "Nil"
    getgenv().masterywpsword = "Nil"
    wait()
    pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Sword" then
                getgenv().namewpsword = v.Name
                getgenv().masterywpsword = v.Level.Value
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Sword" then
                getgenv().namewpsword = v.Name
                getgenv().masterywpsword = v.Level.Value
            end
        end
    end)
end

fruitstorage = ""
for i, v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventoryFruits")) do
    for i1,v1 in next, v do
        if i1 == "Name" then
            fruitstorage = fruitstorage..v1..", "
        end
    end
end

wpstorage = ""
for i, v in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("getInventoryWeapons")) do
    for i1,v1 in next, v do
        if i1 == "Name" then
            wpstorage = wpstorage..v1..", "
        end
    end
end

local suffixes = {"K", "M", "B", "T", "Q","QA"} -- numbers don't go higher than 'Q' in Lua.

local function toSuffixString(n)
	local i = math.floor(math.log(n, 1e3))
	local v = math.pow(10, i * 3)
	return ("%.1f"):format(n / v):gsub("%.?0+$", "") .. (suffixes[i] or "")
end


function sendmsgprofile(url,ValueCheck2)
    checkwpsword()
    wait(1)
    checkwpmelee()
    wait(1)
    fruitstorage = ""
    for i, v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventoryFruits")) do
        for i1,v1 in next, v do
            if i1 == "Name" then
                fruitstorage = fruitstorage..v1..", "
            end
        end
    end
    wpstorage = ""
    for i, v in pairs(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("getInventoryWeapons")) do
        for i1,v1 in next, v do
            if i1 == "Name" then
                wpstorage = wpstorage..v1..", "
            end
        end
    end
    wait(1)
        msg = {
            ["content"] = "concac",
            ["embeds"] = {
                {
                    ["color"] = tonumber(0x000000),
                    ["title"] = ValueCheck2.."- Vin Hub",
                    ["fields"] = {
                        {
                            ["name"] = "Name :",
                            ["value"] = "||"..game.Players.LocalPlayer.Name.."||",
                            ["inline"] = false
                        },
                        {
                            ["name"] = "Frag-Beli-Level",
                            ["value"] = "**Frag :"..toSuffixString(game:GetService("Players").LocalPlayer.Data.Fragments.Value).."\nBeli :"..toSuffixString(game:GetService("Players").LocalPlayer.Data.Beli.Value).."\nLevel :"
                            ..toSuffixString(game:GetService("Players").LocalPlayer.Data.Level.Value).."**",
                            ["inline"] = false,  
                        },
                        {
                            ["name"] = "Weapon Use",
                            ["value"] = "**Melee :"..getgenv().namewpmelee.."\nMastery :"..getgenv().masterywpmelee.."\nSword :"..getgenv().namewpsword.."\nMastery :"..getgenv().masterywpsword.."**",
                            ["inline"] = false
                        },
                        {
                            ["name"] = "Inventory Devil Fruit",
                            ["value"] = "**Inventory Fruit :"..fruitstorage.."**",
                            ["inline"] = false
                        },
                        {
                            ["name"] = "Inventory Weapons",
                            ["value"] = "**Inventory :"..wpstorage.."**",
                            ["inline"] = false
                        },
                    },
                }
            }
        } 
    
    local request = syn.request or http_request
    local response = request(
        {
            Url = url,
            Method = "POST",
            Headers = {
                ["Content-Type"] = "application/json"
            },
            Body = game:GetService("HttpService"):JSONEncode(msg)
        }
    )
end
getgenv().textboxlevel = PSectionMics.AddBox("Url Webhook",{Number_Only = false,DefaultValue = Settingss.urlwebhooklevel},function(v)
    Settingss.urlwebhooklevel = v
end)
OldLevel = game.Players.localPlayer.Data.Level.Value
local locklevel = PSectionMics.AddToggle("WebHook Level MAX",{Default_Value = Settingss.atolocklevel},function(v)
    Settingss.atolocklevel = v
end)
getgenv().webhookdisconect = PSectionMics.AddToggle("WebHook Disconnect",{Default_Value = Settingss.atodisconnect},function(v)
    Settingss.atodisconnect = v
end)
getgenv().atorejoin = PSectionMics.AddToggle("Auto Rejoin",{Default_Value = Settingss.atorejoin},function(v)
    Settingss.atorejoin = v
end)
getgenv().silderminutesprofile = PSectionMics.AddSlider('Time Webhook ',{Min = 0, Max = 2000, Default_Value = Settingss.silderminutesprofiles, Precise = false},function(v)
    Settingss.silderminutesprofiles = v
end)
getgenv().atowebhookprofile = PSectionMics.AddToggle("Webhook Profile", {Default_Value =Settingss.atowebhookprofile},function(v)
    Settingss.atowebhookprofile = v 
    spawn(function()
        while wait() do
            if Settingss.atowebhookprofile then
                pcall(function()
                    tickoldwebhokprofile=tick()
                    repeat wait()
                    until tick()-tickoldwebhokprofile >= Settingss.silderminutesprofiles or not Settingss.atowebhookprofile
                    if Settingss.atowebhookprofile then
                        wait(1)
                        sendmsgprofile(Settingss.urlwebhooklevel,"Check Profile")
                    end
                end)
            end
        end
    end)
end)
getgenv().atofixbrokefarm = PSectionMics.AddToggle("Hop Server when Broke Farm",{Default_Value = Settingss.atofixbrokefarm},function(v)
    Settingss.atofixbrokefarm = v
    spawn(function()
        while wait() do
            if Settingss.atofixbrokefarm then
                pcall(function()
                    tickold=tick()
                    leveloldd=game:GetService("Players").LocalPlayer.Data.Level.Value
                    repeat wait()
                    until tick()-tickold >= 1000 or not Settingss.atofixbrokefarm
                    if game:GetService("Players").LocalPlayer.Data.Level.Value == leveloldd and Settingss.atofixbrokefarm then
                        Teleport()
                    end
                end)
            end
        end
    end)                    
end)
getgenv().atohopserversake = PSectionMics.AddToggle("Hop Server when Same Server",{Default_Value = Settingss.atohopSameServer},function(v)
    Settingss.atohopSameServer = v
    spawn(function()
        while wait() do
            if Settingss.atohopSameServer then
                for i,v in next, Settingsavename.SaveName do
                    if v ~= plr.Name and game.Players:FindFirstChild(v) then
                        Teleport()
                    end
                end
            end
        end
    end)                    
end)
getgenv().atochecktpback = PSectionMics.AddToggle("Auto Check Tp Back",{Default_Value = Settingss.atochecktpback},function(v)
    Settingss.atochecktpback = v
    spawn(function()
        while wait() do
            if Settingss.atochecktpback then
                pcall(function()
                    local starttime = tick()
                    local oldpos = plr.Character.HumanoidRootPart.CFrame.p
                    delay(0.1,function()
                        if tick()-starttime >= 0 and (plr.Character.HumanoidRootPart.CFrame.p-oldpos).Magnitude >= 800 then
                            print(attempt)
                            if attempt >= 2 then
                                tweenmain:Pause()
                                tweenmain:Cancel()
                                tickoldtp = tick()
                                repeat wait()
                                    tweenmain:Pause()
                                    tweenmain:Cancel()
                                until tick()-tickoldtp >= 3
                                attempt = 0
                            else
                                attempt = attempt + 1
                            end
                        end
                    end)
                end)
            end
        end
    end)                    
end)

game:GetService("Players").LocalPlayer.Data.Level.Changed:Connect(function(v)
    if v == 2200 and Settingss.atolocklevel  then
        sendmsg(Settingss.urlwebhooklevel,game:GetService("Players").LocalPlayer.Data.Level.Value,"Level :","Level ")
    end
end)
game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(child)
    if child.Name == 'ErrorPrompt' and child:FindFirstChild('MessageArea') and child.MessageArea:FindFirstChild("ErrorFrame") and Settingss.atodisconnect then
        wait(5)
        if string.find(game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ErrorPrompt.MessageArea.ErrorFrame.ErrorMessage.Text,'Teleport failed, server is full.') then 
        else
            sendmsg(Settingss.urlwebhooklevel,game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ErrorPrompt.MessageArea.ErrorFrame.ErrorMessage.Text,"Reason Disconnect :","Disonnect ")
        end
    end
end)
game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(child)
    if child.Name == 'ErrorPrompt' and child:FindFirstChild('MessageArea') and child.MessageArea:FindFirstChild("ErrorFrame") and Settingss.atorejoin then
        wait(5)
        if string.find(game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ErrorPrompt.MessageArea.ErrorFrame.ErrorMessage.Text,'Teleport failed, server is full.') then 
        else
            while wait() do
                Teleport()
            end
        end
    end
end)
local togclicktp = PSectionMics.AddToggle("Click Teleport",{Default_Value = false},function(v)
    getgenv().clicktp = v
end)

local keybindtp = PSectionMics.AddKeybind('Select Keybind',{Key = Enum.KeyCode.LeftControl},function()
    if getgenv().clicktp then
        local Plr = game:GetService("Players").LocalPlayer
        local Mouse = Plr:GetMouse()
        Plr.Character:MoveTo(Mouse.Hit.p)
    end
end)



local infinitestam = PSectionMics.AddButton('Infinity Stamina', function()
    local mt = getrawmetatable(game)
    local old = mt.__index
    setreadonly(mt, false)
    mt.__index = newcclosure(function(t, k)
        if k == 'Value' and t.Name == 'Energy' then
            return math.huge
        end
        return old(t, k)
    end)
    setreadonly(mt, true)
end)

local dashcooldown = PSectionMics.AddToggle('No Dash Cooldown', {Default_Value = false}, function(n)
    getgenv().dash = v
    if getgenv().dash then
        for i, v in next, getreg() do
            if type(v) == 'function' and getfenv(v).script == plr.Character.Dodge then
                for i2, v2 in next, debug.getconstants(v) do
                    if v2 == 0.4 then
                        debug.setconstant(v, i2, 0)
                    end
                end
            end
        end
    else
        for i, v in next, getreg() do
            if type(v) == 'function' and getfenv(v).script == plr.Character.Dodge then
                for i2, v2 in next, debug.getconstants(v) do
                    if v2 == 0 then
                        debug.setconstant(v, i2, 0.4)
                    end
                end
            end
        end
    end
end)

local SettingPage = a.AddPage('Setting')

local SSectionFarm = SettingPage.AddSection("Farm Setting")
local toggleautosave = SSectionFarm.AddToggle("Auto Save ",{Default_Value = Settingss.savee},function(v)
    Settingss.savee = v
end)
spawn(function()
    while wait() do
        pcall(function()
            if Settingss.savee then
                writefile("Vin Hub/BloxFruit.json", HttpService:JSONEncode(Settingss))
            else
                writefile("Vin Hub/BloxFruit.json", HttpService:JSONEncode(DefaultSettings))
            end
        end)
    end
end)
