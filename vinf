function sendmsg(url,Sword)
    msg = {
        ["content"] = "@everyone",
        ["embeds"] = {
            {
            ["color"] = tonumber(0x000000),
            ["title"] = "Hop Legend Sword - Vin Hub",
            ["fields"] = {
                {
                ["name"] = "Name :",
                ["value"] = "||"..game.Players.LocalPlayer.Name.."||",
                ["inline"] = false
            },
            {
            ["name"] = "Sword :",
            ["value"] = Sword,
            ["inline"] = false,  
            }
            },
            }
        }
    } 
    local request = syn.request or http_request
    local response = request(
    {
    Url = url,
    Method = "POST",
    Headers = {
    ["Content-Type"] = "application/json"
    },
    Body = game:GetService("HttpService"):JSONEncode(msg)
    }
    )
end

getgenv().countthop = 0
getgenv().hoplegend = MSectionMics.AddToggle("Auto Hop Sword Legend",{Default_Value = Settingss.atohoplegend},function(v)
    Settingss.atohoplegend = v
    spawn(function()
        while wait() do
            pcall(function()
                if Settingss.atohoplegend then
                    repeat wait(1)
                        getgenv().countthop = getgenv().countthop + 1
                        legensw()
                    until getgenv().countthop >= 15
                    Teleport()
                end
            end)
        end
    end)
end)

getgenv().textboxhoplegend = MSectionMics.AddBox("Url Webhook",{Number_Only = false,DefaultValue = Settingss.boxhoplegend},function(v)
    Settingss.boxhoplegend = v
end)

game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(v)
    if v:IsA("Tool") and v.ToolTip == "Sword" and Settingss.atohoplegend and Settingss.boxhoplegend ~= nil then
        sendmsg(Settingss.boxhoplegend,v.Name)
    end
end)
end

