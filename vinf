if  game.PlaceId == 4442272183 or game.PlaceId == 7449423635 then
    local function df()
        local abringdf
    
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do 
            if v:FindFirstChild("EatRemote") then
                abringdf = game.Players.LocalPlayer.Backpack
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do 
            if v:FindFirstChild("EatRemote") then
                abringdf = game.Players.LocalPlayer.Character
            end
        end
    
        return abringdf
    end
    
    local function hoop()
        local abringdf = df()
        if abringdf == nil then
            for i,cailon in pairs(game.Workspace:GetChildren()) do
                if not cailon:IsA("Tool") and not string.find(cailon.Name, "Fruit") then
                    if not plr.Backpack:FindFirstChild("Special Microchip") and not plr.PlayerGui.Notifications:FindFirstChild("NotificationTemplate") and not plr.PlayerGui.Main.Timer.Visible then
                        Teleport()
                    end
                end
            end
        end
    end
    local oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
    local function AutoRaidshop()
        pcall(function()
            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if string.find(v.Name, "Fruit") then
                    if not plr.PlayerGui.Notifications:FindFirstChild("NotificationTemplate") and not plr.PlayerGui.Main.Timer.Visible and not plr.Backpack:FindFirstChild("Special Microchip") then
                            Remote("RaidsNpc", "Check")
                            Remote("RaidsNpc", "Select", Settingss.dfraid)
                            oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
                    end
                end
            end
            if plr.Backpack:FindFirstChild("Special Microchip") and not plr.PlayerGui.Main.Timer.Visible then
                if getgenv().Farm2 then
                    getgenv().Farm = false
                end
                tweenmain:Pause()
                tweenmain:Cancel()
                getgenv().ps = nil
                getgenv().Island1 = true
                getgenv().Island2 = false
                getgenv().Island3 = false
                getgenv().Island4 = false
                getgenv().Island5 = false
                if game.PlaceId == 7449423635 then
                    wait(3)
                    tweenmain:Pause()
                    tweenmain:Cancel()
                        fireclickdetector(game:GetService("Workspace").Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
                    oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
                end
                if game.PlaceId == 4442272183 then
                    wait(3)
                    tweenmain:Pause()
                    tweenmain:Cancel()
                    fireclickdetector(game:GetService("Workspace").Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
                    oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
                end
                wait(0.5)
            end
            
            for i,v in pairs(plr.PlayerGui.Notifications:GetDescendants()) do
                if v:IsA("TextLabel") then
                    if string.find(v.Text,"1000") and getgenv().Farm2 then
                        tweenmain:Pause()
                        tweenmain:Cancel()
                        getgenv().Farm = true
                        getgenv().Island5 = false
                    end
                end
            end
    
            if plr.PlayerGui.Main.Timer.Visible then
                if getgenv().Farm2 then
                    getgenv().Farm = false
                end
                for i,v in pairs(game:GetService("Workspace")["_WorldOrigin"].Locations:GetChildren()) do
                    if v:IsA("Part") then
                        if  v.Name == "Island 1"  then
                            if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 3000   then
                                if getgenv().Island1 and getgenv().Island2 == false and getgenv().Island3 == false and getgenv().Island4 == false and getgenv().Island5 == false then
                                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.Position,v.CFrame*CFrame.new(0,60,0))
                                end
                            end
                        end
                        if  v.Name == "Island 2"  then
                            if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 3000   then
                                getgenv().Island2 = true
                                getgenv().Island1 = false
                                if getgenv().Island2 and getgenv().Island1 == false and getgenv().Island3 == false and getgenv().Island4 == false and getgenv().Island5 == false then
                                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.Position,v.CFrame*CFrame.new(0,60,0))
                                end
                            end
                        end
                        if  v.Name == "Island 3"  then
                            if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 3000   then
                                getgenv().Island2 = false
                                getgenv().Island3 = true
                                if getgenv().Island3 and getgenv().Island1 == false and getgenv().Island2 == false and getgenv().Island4 == false and getgenv().Island5 == false then
                                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.Position,v.CFrame*CFrame.new(0,60,0))
                                end
                            end
                        end
                        if  v.Name == "Island 4"  then
                            if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 3000   then
                                getgenv().Island3 = false
                                getgenv().Island4 = true
                                if getgenv().Island4 and getgenv().Island1 == false and getgenv().Island2 == false and getgenv().Island3 == false and getgenv().Island5 == false then
                                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.Position,v.CFrame*CFrame.new(0,60,0))
                                end
                            end
                        end
                        if  v.Name == "Island 5" then
                            if (v.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 2000   then
                                getgenv().Island4 = false
                                getgenv().Island5 = true
                                if getgenv().Island5 and getgenv().Island1 == false and getgenv().Island2 == false and getgenv().Island3 == false and getgenv().Island4 == false then
                                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.Position,v.CFrame*CFrame.new(0,60,0))
                                end
                            end
                        end
                    end
                end
            end
    
            if plr.PlayerGui.Main.Timer.Visible then
                for i,v in pairs(game.Workspace.Enemies:GetDescendants()) do
                    if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and isnetworkowner(v.HumanoidRootPart) and v.Humanoid.Health > 0 then
                        repeat wait()
                            v.Humanoid.Health = 0
                        until not v.Parent or v.Humanoid.Health <= 0 or not plr.PlayerGui.Main.Timer.Visible
                            if oldfrag ~= game:GetService("Players").localPlayer.Data.Fragments.Value then
                            end
                    end
                end
            end
        end)
    end
    
    local function bringevilfhop()
        for i,v in pairs(game.Workspace:GetChildren()) do
            if v:IsA("Tool") and string.find(v.Name, "Fruit")  then
                v.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,50)
                wait(0.5)
                firetouchinterest(game.Players.LocalPlayer.Character.Head,v.Handle,0)
            end
        end    
    end
    local function equip()
        for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") and not string.find(v.Name, "Fruit") then
                game:GetService('Players').LocalPlayer.Character:WaitForChild('Humanoid'):EquipTool(game:GetService('Players').LocalPlayer:WaitForChild('Backpack'):FindFirstChild(v.Name))
            end
        end
    end
    
