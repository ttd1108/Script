local function removeduplicates(b)
    local c = {}
    local d = {}
    for e, f in ipairs(b) do
        if not d[f] then
            d[f] = true
            table.insert(c, f)
        end
    end
    return c
end

local mobA = {}

for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
    if v:IsA('Model') and v:FindFirstChild('Humanoid') and v:FindFirstChild('HumanoidRootPart') then
        table.insert(mobA, v.Name)
    end
end

for i, v in pairs(game.ReplicatedStorage:GetChildren()) do
    if v:IsA('Model') and v:FindFirstChild('Humanoid') and v:FindFirstChild('HumanoidRootPart') then
        table.insert(mobA, v.Name)
    end
end

mobB = {}

mobC = removeduplicates(mobA)
for i = 1, #mobC do
    table.insert(mobB, mobC[i])
end

local function sort(o)
    if o then
        function compare(a, b)
            if a:match('%d+') and b:match('%d+') then
                return tonumber(a:match('%d+')) < tonumber(b:match('%d+'))
            end
        end
        table.sort(o, function(a, b)
            return compare(a, b)
        end)
        return o
    end
end

mobs = {}

for i, v in pairs(sort(mobB)) do
    table.insert(mobs, v)
end

local LandsA = {}

for i,v in pairs(game.Workspace["_WorldOrigin"].Locations:GetChildren()) do
    if v.Name ~= "Sea" then
        table.insert(LandsA, v.Name)
    end
end

local Lands = {}

LandsC = removeduplicates(LandsA)
for i = 1, #LandsC do
    table.insert(Lands, LandsC[i])
end

local function Remote(x, v, c)
    if not x and c then
        CommF:InvokeServer(v, true)
    else
        CommF:InvokeServer(x, v, c)
    end
end

local function waitdie()
    repeat
        wait()
    until game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild('Humanoid') and
        game.Players.LocalPlayer.Character.Humanoid.Health > 0
    return game.Players.LocalPlayer.Character
end

if game.PlaceId == 4442272183 then
    for i, v in pairs(game:GetService("Workspace").Map.SnowMountain:GetChildren()) do
        if v.Name == "Base" then
            if v.BrickColor == BrickColor.new("Sand blue") then
                v:Destroy()
            end
        elseif v.Name == "Details" then
            v:Destroy()
        end
    end
elseif game.PlaceId == 7449423635 then
    for i, v in pairs(game:GetService("Workspace").Map.Turtle:GetDescendants()) do
        if string.find(v.Name, "Grass") and v.CanCollide == false then
            v.CanCollide = true
        end
    end
end
local function equiptool(toolname)
    pcall(function()
        waitdie()
        if game:GetService('Players').LocalPlayer:WaitForChild('Backpack'):FindFirstChild(toolname) and  game:GetService('Players').LocalPlayer.Character:FindFirstChild("Humanoid") and game:GetService('Players').LocalPlayer.Character.Humanoid.Health > 0 then
            game:GetService('Players').LocalPlayer.Character:WaitForChild('Humanoid'):EquipTool(game:GetService('Players').LocalPlayer:WaitForChild('Backpack'):FindFirstChild(toolname))
        end
    end)
end

function cailonquetest()
	local mobconcacgido = {}
	for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
		if not table.find(mobconcacgido, v.Name) and v:IsA('Model') and v:FindFirstChild('Humanoid') and v:FindFirstChild('HumanoidRootPart') then
			table.insert(mobconcacgido, v.Name)
		end
	end

	for i, v in pairs(game.ReplicatedStorage:GetChildren()) do
		if not table.find(mobconcacgido, v.Name) and v:IsA('Model') and v:FindFirstChild('Humanoid') and v:FindFirstChild('HumanoidRootPart') then
			table.insert(mobconcacgido, v.Name)
		end
	end
	return mobconcacgido
end
local function checkquest()
    local max = plr.Data.Level.Value
    local min = 0
    local mob;
    local a;
    for i,v in pairs(quest) do
        for i1,v1 in pairs(v) do
            local lvlreq = v1.LevelReq
            for i2,v2 in pairs(v1.Task) do
                if max >= lvlreq and lvlreq >= min and v1.Task[i2] > 1 and tostring(i) ~= 'BartiloQuest' and tostring(i) ~= 'Trainees' and tostring(i) ~= 'MarineQuest' and tostring(i) ~= 'CitizenQuest' then
                    min = lvlreq
                    a = tostring(i2)
                end
            end
        end
    end
    for i, v in pairs(cailonquetest()) do
        local mob1 = v
        v = v:gsub(' %pLv. %d+%p', '')
        v = v:gsub(' %pBoss%p', '')
        if v == a then
            mob = tostring(mob1)
        end
    end
    return mob
end
local tweenmain
local attempt = 0
pcall(function()
function toTarget(pos, targetPos, targetCFrame)
    local tween_s = game:service"TweenService"
    local info = TweenInfo.new((targetPos - pos).Magnitude/Settingss.speedtween, Enum.EasingStyle.Linear)
    if game.Players.LocalPlayer.Character.Humanoid.Sit then
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
    end
    if plr.Character.Humanoid.Health > 0 then
        tweenmain = tween_s:Create(game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart"), info, {CFrame = targetCFrame})
        tweenmain:Play()
    end
end

toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)

local tweenrach
function tweencomplete(targetCFrame)
    local gay = (targetCFrame.p-game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).Magnitude
    tweenrach = game:GetService('TweenService'):Create(
        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),
        TweenInfo.new(
            gay / Settingss.speedtween, 
            Enum.EasingStyle.Linear
        ),
        {CFrame = targetCFrame}
    )
    tweenrach:Play()
    tweenrach.Completed:Wait()
end 
toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
end)
local t = require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework.CameraShaker)
t.Shake = function() end
function teleportautofarm(v)
    if (v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 50 and plr.Character.Humanoid.Health > 0 then
        tweenmain:Pause()
        tweenmain:Cancel()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame *CFrame.new(0,20,0)
    else
        if  game.PlaceId == 2753915549 and (game:GetService("Workspace").Map.SkyArea2.PathwayHouse.EntrancePoint.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 3000 and plr.Data.Level.Value >= 475 and plr.Data.Level.Value < 525  then
            wait()
            tweenmain:Pause()
            tweenmain:Cancel()
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance", game:GetService("Workspace").Map.SkyArea2.PathwayHouse.EntrancePoint.Position);
            wait(1)
        elseif game.PlaceId == 2753915549 and (game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 5000 and plr.Data.Level.Value >= 375 and plr.Data.Level.Value < 450  then
            wait()
            tweenmain:Pause()
            tweenmain:Cancel()
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.Position);
            wait(1)
		elseif game.PlaceId == 4442272183 and (game:GetService("Workspace").Map.GhostShipInterior.TeleportSpawn.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 5000 and plr.Data.Level.Value >= 1250 and plr.Data.Level.Value < 1350  then
            wait()
            tweenmain:Pause()
            tweenmain:Cancel()
            game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.GhostShipInterior.TeleportSpawn.Position);
            wait(1)
		else
            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame)
        end
    end
end
local function getquest(mob)
    for i,v in pairs(quest) do
        for i2, v2 in pairs(v) do
            if v2.Task[mob] and tostring(i) ~= 'BartiloQuest' and tostring(i) ~= 'MarineQuest' and tostring(i) ~= 'Trainees' and tostring(i) ~= "CitizenQuest" then
                if (questpoint[tostring(i)].Position-plr.Character.HumanoidRootPart.Position).magnitude <= 8 then
                    wait(0.5)
                    Remote('StartQuest', tostring(i), i2)
				else
					if game.PlaceId == 2753915549 and (game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 5000 and plr.Data.Level.Value >= 375 and plr.Data.Level.Value < 450  then
						wait(1)
						tweenmain:Pause()
						tweenmain:Cancel()
						game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.Position);
						wait(2)
					elseif game.PlaceId == 2753915549 and  (game:GetService("Workspace").Map.TeleportSpawn.EntrancePoint.Position-plr.Character.HumanoidRootPart.Position).Magnitude < 5000 and plr.Data.Level.Value >= 450 then
						wait()
						tweenmain:Pause()
						tweenmain:Cancel()
						game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.TeleportSpawn.ExitPoint.Position);
						wait(2)
					elseif game.PlaceId == 2753915549 and (game:GetService("Workspace").Map.SkyArea1.PathwayTemple.ExitPoint.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 2000 and plr.Data.Level.Value >= 450 and plr.Data.Level.Value < 525  then
						wait(1)
						game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance", game:GetService("Workspace").Map.SkyArea1.PathwayTemple.ExitPoint.Position);
						tweenmain:Pause()
						tweenmain:Cancel()
						wait(2)
					elseif game.PlaceId == 2753915549 and (game:GetService("Workspace").Map.SkyArea2.PathwayHouse.EntrancePoint.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 5000 and plr.Data.Level.Value >= 525 and plr.Data.Level.Value < 625  then
						wait()
						tweenmain:Pause()
						tweenmain:Cancel()
						game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance", game:GetService("Workspace").Map.SkyArea2.PathwayHouse.EntrancePoint.Position);
						wait(2)
					elseif game.PlaceId == 4442272183 and (game:GetService("Workspace").Map.GhostShipInterior.TeleportSpawn.Position-plr.Character.HumanoidRootPart.Position).Magnitude > 5000 and plr.Data.Level.Value >= 1250 and plr.Data.Level.Value < 1350  then
						wait(1)
						tweenmain:Pause()
						tweenmain:Cancel()
						game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.GhostShipInterior.TeleportSpawn.Position);
						wait(2)
					elseif game.PlaceId == 4442272183 and (game:GetService("Workspace").Map.GhostShipInterior.TeleportSpawn.Position-plr.Character.HumanoidRootPart.Position).Magnitude < 5000 and plr.Data.Level.Value >= 1350 then
						wait(1)
						tweenmain:Pause()
						tweenmain:Cancel()
						game.ReplicatedStorage.Remotes.CommF_:InvokeServer("requestEntrance",game:GetService("Workspace").Map.GhostShip.TeleportSpawn.Position);
						wait(2)
					else
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,questpoint[tostring(i)].Position,questpoint[tostring(i)])
					end
                end
            end
        end
    end
end

local function sizepart(v)
    v.HumanoidRootPart.CanCollide = false
    v.HumanoidRootPart.Size = Vector3.new(25, 0, 25)
    v.HumanoidRootPart.Transparency = 1
    v.HumanoidRootPart.BrickColor = BrickColor.new("White")
    v.Humanoid.WalkSpeed = 0
    if not v.HumanoidRootPart:FindFirstChild("EffectsSY") then
        local BV = Instance.new("BodyVelocity")
        BV.Parent = v.HumanoidRootPart
        BV.Name = "EffectsSY"
        BV.MaxForce = Vector3.new(100000, 100000, 100000)
        BV.Velocity = Vector3.new(0, 0, 0)
    end
end


local ignored = {game:GetService("Workspace").Characters,game:GetService("Workspace").Enemies}
function RayCast2(a,b,c)
    local raycastParameters = RaycastParams.new()
    raycastParameters.FilterDescendantsInstances = ignored
    raycastParameters.FilterType = Enum.RaycastFilterType.Blacklist
    raycastParameters.IgnoreWater = false
    local rels = workspace:Raycast(a,b,raycastParameters)
    return rels
end
local Lv = {"A","NB","N"}
function GetPosPlus(cf) 
    local List = {
        ["A"] = CFrame.new(0,Settingss.distancefarmm,0),
        ["NB"] = CFrame.new(0,0,-Settingss.distancefarmm),
        ["N"] = CFrame.new(Settingss.distancefarmm,0,0)
    }
    for k,v in pairs(Lv) do
        local new = cf*List[v]
        local t = RayCast2(cf.p,(new.p-cf.p).Unit*(new.p-cf.p).magnitude)
        local tr = true
        if not t then 
            tr=false
        end
        if not tr then 
            return List[v]
        end
    end
    return CFrame.new(0,Settingss.distancefarmm,0)
end
function autoattack()
    if toolrac.activeController and toolrac.activeController.anims and toolrac.activeController.anims.basic then                    
        toolrac.activeController.hitboxMagnitude= 30
        toolrac.activeController.attacking = false
        toolrac.activeController.timeToNextAttack = 0                      
        toolrac.activeController.blades = {game.Players.LocalPlayer.Character.HumanoidRootPart}
        toolrac.activeController.increment = 2
        spawn(function() 
            toolrac.activeController:attack()                                   
        end)                 
    end
end
function checkproautofarm(v)
    local concac = Instance.new("Part")
    concac.Name = "Test"
    concac.Parent = v.HumanoidRootPart
end
function notequipautofarm()
    spawn(function()
        if  Settingss.autosuperhuman  or  Settingss.AutoSw then
        else
            equiptool(Settingss.tool)
        end
    end)
end
 function autoattackcheck()
    if getgenv().ps ~= nil and (plr.Character.HumanoidRootPart.Position-getgenv().ps).Magnitude <= Settingss.distancefarmm + 5 then
        autoattack()
    end
end
local function autofarm()
    if plr.Data.Level.Value >= 1500 and game.PlaceId == 4442272183 then
        mob = "Water Fighter"
        getgenv().mob = "Water Fighter [Lv. 1450]"
        
    elseif game:GetService("Players").LocalPlayer.Data.Level.Value >= 800 and game:GetService("Players").LocalPlayer.Data.Level.Value < 875 and Settingss.atoskipstafffactory and game.PlaceId == 4442272183 then
        mob = "Swan Pirate"
        getgenv().mob = "Swan Pirate [Lv. 775]"
    elseif game:GetService("Players").LocalPlayer.Data.Level.Value >= 1575 and game:GetService("Players").LocalPlayer.Data.Level.Value < 1600 and game.PlaceId == 7449423635 and Settingss.atoskipDragoncrew then
        mob = "Pistol Billionaire"
        getgenv().mob = "Pistol Billionaire [Lv. 1525]"
    elseif game:GetService("Players").LocalPlayer.Data.Level.Value >= 1700 and game:GetService("Players").LocalPlayer.Data.Level.Value < 1725 and game.PlaceId == 7449423635 and Settingss.atoskiplvl1700 then
        mob = "Giant Islander"
        getgenv().mob = "Giant Islander [Lv. 1650]"
    else
        mob = checkquest()
        mob = mob:gsub(" %pLv. %d+%p", "")
        mob = mob:gsub(" %pBoss%p", "")
        getgenv().mob = checkquest()
    end
    if getgenv().Farm then
        if plr.PlayerGui.Main:FindFirstChild("Quest").Visible == false  or not string.find(plr.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, mob) then
            if not string.find(plr.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, mob)  then
                getgenv().getcentermob = nil
            end
            getgenv().ps = nil
            getquest(mob)
            
        else
            local folderfarm = game.workspace.Enemies
            if game.ReplicatedStorage:FindFirstChild(getgenv().mob) then
                folderfarm = game.ReplicatedStorage
            end
            for i,v in pairs(folderfarm:GetChildren()) do
                pcall(function()
                    if v.Name == getgenv().mob and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0  then
                        repeat wait()
                            autoattackcheck()
                            getgenv().ps = v.HumanoidRootPart.Position
                            teleportautofarm(v)
                            sizepart(v)
                            notequipautofarm()
                        until v.Humanoid.Health <= 0 or not getgenv().Farm or plr.PlayerGui.Main:FindFirstChild("Quest").Visible == false  or not v.Parent
                        getgenv().ps =nil
                    end
                end)
            end
        end
    end
end

function getcenter(mob)
    local MaxX,MaxY,MaxZ = -math.huge,-math.huge,-math.huge
    local MinX,MinY,MinZ = math.huge,math.huge,math.huge
    if getgenv().getcentermob == nil then
        local folderbring = game.workspace.Enemies
        if game.ReplicatedStorage:FindFirstChild(mob) then
            folderbring = game.ReplicatedStorage
        end
        for i,v in next, folderbring:GetChildren() do
            if v.Name == mob and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and isnetworkowner(v.HumanoidRootPart) then
                if (v.HumanoidRootPart.CFrame.X > MaxX) then MaxX = v.HumanoidRootPart.CFrame.X end
                if (v.HumanoidRootPart.CFrame.Y > MaxY) then MaxY = v.HumanoidRootPart.CFrame.Y end
                if (v.HumanoidRootPart.CFrame.Z > MaxZ) then MaxZ = v.HumanoidRootPart.CFrame.Z end
                if (v.HumanoidRootPart.CFrame.X < MinX) then MinX = v.HumanoidRootPart.CFrame.X end
                if (v.HumanoidRootPart.CFrame.Y < MinY) then MinY = v.HumanoidRootPart.CFrame.Y end
                if (v.HumanoidRootPart.CFrame.Z < MinZ) then MinZ = v.HumanoidRootPart.CFrame.Z end
            end
        end
        cac1 = Vector3.new(MaxX,MaxY,MaxZ)
        cac2 = Vector3.new(MinX,MinY,MinZ)
    end
    return cac2:Lerp(cac1,0.5)
end


local function bring()
    for i, v1 in pairs(game.workspace.Enemies:GetChildren()) do
        if v1.Name == getgenv().mob  and (getgenv().Farm or getgenv().mastery or getgenv().masteryGun) then
            if v1.Name == "Chief Petty Officer [Lv. 120]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0   then
                v1.HumanoidRootPart.CFrame = CFrame.new(-4786.16796875, 19.350784301758, 4229.943359375)
                sizepart(v1)
            elseif v1.Name == "Raider [Lv. 700]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-722.859, 37.9342, 2357.37)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-722.859, 37.9342, 2357.37)
                    sizepart(v1)
                elseif (Vector3.new(351.959, 37.9349, 2340.8)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(351.959, 37.9349, 2340.8)
                    sizepart(v1)
                end
            elseif v1.Name == "Mercenary [Lv. 725]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-960.67034912109, 78.707794189453, 1691.1026611328)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-960.67034912109, 78.707794189453, 1691.1026611328)
                    sizepart(v1)
                elseif (Vector3.new(-1093.35, 71.7541, 1194.99)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-1093.35, 71.7541, 1194.99)
                    sizepart(v1)
                end
            elseif v1.Name == "Factory Staff [Lv. 800]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(681.841, 71.7541, 104.373)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(681.841, 71.7541, 104.373)
                    sizepart(v1)
                elseif (Vector3.new(-153.039, 148.227, -359.545)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-153.039, 148.227, -359.545)
                    sizepart(v1)
                end
            elseif v1.Name == "Marine Lieutenant [Lv. 875]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-2918.2, 71.7605, -2949.49)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-2918.2, 71.7605, -2949.49)
                    sizepart(v1)
                end
            elseif v1.Name == "Marine Captain [Lv. 900]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-1812.6, 71.7605, -3403.54)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-1812.6, 71.7605, -3403.54)
                    sizepart(v1)
                end
            elseif v1.Name == "Vampire [Lv. 975]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-6037.84, 5.19706, -1309.44)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-6037.84, 5.19706, -1309.44)
                    sizepart(v1)
                end
            elseif v1.Name == "Lava Pirate [Lv. 1200]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-5216.1, 27.7263, -4641.44)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-5216.1, 27.7263, -4641.44)
                    sizepart(v1)
                end
            elseif v1.Name == "Ship Deckhand [Lv. 1250]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (v1.HumanoidRootPart.Position-getgenv().ps).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(getgenv().ps)
                    sizepart(v1)
                end
            elseif v1.Name == "Ship Officer [Lv. 1325]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(616.486, 179.943, 33291.7)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(616.486, 179.943, 33291.7)
                    sizepart(v1)
                elseif (Vector3.new(1173.64, 179.942, 33309.7)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(1173.64, 179.942, 33309.7)
                    sizepart(v1)
                end
            elseif v1.Name == "Sea Soldier [Lv. 1425]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (v1.HumanoidRootPart.Position-getgenv().ps).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(getgenv().ps)
                    sizepart(v1)
                end
            elseif v1.Name == "Dragon Crew Warrior [Lv. 1575]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(4133.41, 51.8747, -1277.17)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(4133.41, 51.8747, -1277.17)
                    sizepart(v1)
                elseif (Vector3.new(6515.75, 51.8891, -973.228)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(6515.75, 51.8891, -973.228)
                    sizepart(v1)
                elseif (Vector3.new(6212.19, 51.9133, -1672.95)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(6212.19, 51.9133, -1672.95)
                    sizepart(v1)
                end
            elseif v1.Name == "Dragon Crew Archer [Lv. 1600]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(6548.3173828125, 378.79486083984, 154.83052062988)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(6548.3173828125, 378.79486083984, 154.83052062988)
                    sizepart(v1)
                end
            elseif v1.Name == "Marine Rear Admiral [Lv. 1725]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(3671.87, 160.917, -7014.49)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(3671.87, 160.917, -7014.49)
                    sizepart(v1)
                elseif (Vector3.new(3230.46, 124.921, -6738.41)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(3230.46, 124.921, -6738.41)
                    sizepart(v1)
                end
            elseif v1.Name == "Fishman Captain [Lv. 1800]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-10983.5, 332.155, -8937.3)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-10983.5, 332.155, -8937.3)
                    sizepart(v1)
                end
            elseif v1.Name == "Forest Pirate [Lv. 1825]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if (Vector3.new(-13375.044921875, 332.77099609375, -7824.5219726562)-v1.HumanoidRootPart.Position).Magnitude < 360 then
                    v1.HumanoidRootPart.CFrame = CFrame.new(-13375.044921875, 332.77099609375, -7824.5219726562)
                    sizepart(v1)
                end
            elseif isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid") and v1.Humanoid.Health > 0  then
                if game.PlaceId == 7449423635 then
                    sizepart(v1)
                    if  plr.Data.Level.Value >= 1925 and plr.Data.Level.Value <= 2200 then
                        if getgenv().getcentermob == nil and (plr.Character.HumanoidRootPart.Position-getgenv().ps).Magnitude < 100 then
                            getgenv().getcentermob = getcenter(v1.Name)
                        end
                            v1.HumanoidRootPart.CFrame = CFrame.new(getgenv().getcentermob)
                    else
                        v1.HumanoidRootPart.CFrame = CFrame.new(getgenv().ps)
                    end
                else
                    if getgenv().getcentermob == nil and (plr.Character.HumanoidRootPart.Position-getgenv().ps).Magnitude < 100 then
                        getgenv().getcentermob = getcenter(v1.Name)
                    end
                        v1.HumanoidRootPart.CFrame = CFrame.new(getgenv().getcentermob)
                    sizepart(v1)
                end
            end
        end
    end
end





local function FFCMatch(Ins, Name)
    for k,v in pairs(Ins:GetChildren()) do
        if string.match(v.Name, Name) then
            return v
        end
    end
    return nil
end

local function AutoBuso()
    if FFCMatch(plr.Character, "_BusoLayer1") == nil and not plr.Character:FindFirstChild("HasBuso") then
        game.ReplicatedStorage.Remotes["CommF_"]:InvokeServer("Buso")
    end
end

local Weapon = {}

pcall(function()
    for i, v in pairs(plr.Backpack:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == "Gun" or v.ToolTip == "Melee" or v.ToolTip == "Sword" then
            table.insert(Weapon, v.Name)
        end
    end

    for i, v in pairs(game.Workspace.Characters[plr.Name]:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == "Gun" or v.ToolTip == "Melee" or v.ToolTip == "Sword" then
            table.insert(Weapon, v.Name)
        end
    end
end)

function checkdevilfruit()
    getgenv().checkdff = nil
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if string.find(v.Name,"Fruit")  then
            getgenv().checkdff = v.Name
        end
    end
    return getgenv().checkdff
end


getgenv().tablefruit = {
    "Quake-Quake",
    "Human-Human : Buddha",
    "String-String",
    "Bird-Bird: Phoenix",
    "Rumble-Rumble",
    "Paw-Paw",
    "Gravity-Gravity",
    "Dough-Dough",
    "Shadow-Shadow",
    "Venom-Venom",
    "Control-Control",
    "Dragon-Dragon"
}
function AutoRaidsfully()
    pcall(function()
        if not game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") and plr.Data.Level.Value >= 1100 and not plr.PlayerGui.Notifications:FindFirstChild("NotificationTemplate") and not plr.PlayerGui.Main.Timer.Visible and not plr.Backpack:FindFirstChild("Special Microchip") then
            wait(3)
            if checkdevilfruit() == nil and Settingss.togglegetfruit then
                for i, v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventoryFruits")) do
                    for i1,v1 in pairs(v) do
                        if i1 == "Name" then
                            if not table.find(getgenv().tablefruit,v1) then
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LoadFruit",v1)
                            end
                        end
                    end
                end
            end
            wait()
            Remote("RaidsNpc", "Check")
            Remote("RaidsNpc", "Select", "Flame")
        end
        if plr.Backpack:FindFirstChild("Special Microchip") and not plr.PlayerGui.Main.Timer.Visible then
            if getgenv().Farm2 then
                getgenv().Farm = false
            end
            tweenmain:Pause()
            tweenmain:Cancel()
            getgenv().ps = nil
            if game.PlaceId == 7449423635 then
                wait(3)
                tweenmain:Pause()
                tweenmain:Cancel()
                    fireclickdetector(game:GetService("Workspace").Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
                oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
            end
            if game.PlaceId == 4442272183 then
                wait(3)
                tweenmain:Pause()
                tweenmain:Cancel()
                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
                oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
            end
            wait(0.5)
        end
        
        for i,v in pairs(plr.PlayerGui.Notifications:GetDescendants()) do
            if v:IsA("TextLabel") then
                if string.find(v.Text,"1000") and getgenv().Farm2 then
                    tweenmain:Pause()
                    tweenmain:Cancel()
                    getgenv().Farm = true
                end
            end
        end

        if plr.PlayerGui.Main.Timer.Visible then
            if getgenv().Farm2 then
                getgenv().Farm = false
            end
            if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame * CFrame.new(0,80,0))
            end
        end

        if plr.PlayerGui.Main.Timer.Visible then
            for i,v in pairs(game.Workspace.Enemies:GetDescendants()) do
                if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude < 400  and v.Humanoid.Health > 0 then
                    repeat wait()
                        v.Humanoid.Health = 0
                    until not v.Parent or v.Humanoid.Health <= 0 or not plr.PlayerGui.Main.Timer.Visible or not Settingss.atofully
                end
            end
        end
    end)
end

local function AutoSuperHuman(path)
    for i, v in pairs(path:GetChildren()) do
        if v:IsA("Tool") then
            if v.Name == "Combat" then
                equiptool(v.Name)
                if plr.Data.Beli.Value >= 150000 then
                    plr.Character.Humanoid.Health = 0
                    Remote("BuyBlackLeg")
                end
            elseif v.Name == "Black Leg" then
                equiptool(v.Name)
                if v.Level.Value >= 300 then
                    Remote("BuyFishmanKarate")
                end
            elseif v.Name == "Fishman Karate" then
                equiptool(v.Name)
                if v.Level.Value >= 300 then
                    Remote("BuyElectro")
                end
            elseif v.Name == "Electro" then
                equiptool(v.Name)
                if v.Level.Value >= 300 and game:GetService("Players").localPlayer.Data.Fragments.Value >= 1500 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BlackbeardReward","DragonClaw","2") == 0  then
                    plr.Character.Humanoid.Health = 0
                    Remote("BlackbeardReward", "DragonClaw", "1")
                    Remote("BlackbeardReward", "DragonClaw", "2")
                elseif v.Level.Value >= 300  then
                    Remote("BlackbeardReward", "DragonClaw", "1")
                    Remote("BlackbeardReward", "DragonClaw", "2")
                end
                if v.Level.Value >= 300 and plr.Data.Level.Value >= 1100 and game:GetService("Players").localPlayer.Data.Fragments.Value < 1500 and Settingss.atofully then
                    AutoRaidsfully()
                end
            elseif v.Name == "Dragon Claw" then
                equiptool(v.Name)
                if v.Level.Value >= 300 then
                    Remote("BuySuperhuman")
                end
            elseif v.Name == "Superhuman" then
                equiptool(v.Name)
            end
        end
    end
end


function checkkeylibrary()
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v.Name == "Library Key" then
			getgenv().namekey = v.Name
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v.Name == "Library Key" then
			getgenv().namekey = v.Name
		end
	end
	return getgenv().namekey
end
function checkLevelBlackLeg()
    if Settingss.atodeadstep then
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v.Name == "Black Leg" then
                getgenv().masblackleg = v.Level.Value
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.Name == "Black Leg" then
                getgenv().masblackleg = v.Level.Value
            end
        end
    end
    if Settingss.atosharkmanv2 then
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v.Name == "Fishman Karate" then
                getgenv().masblackleg = v.Level.Value
            end
        end
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.Name == "Fishman Karate" then
                getgenv().masblackleg = v.Level.Value
            end
        end
    end

	return getgenv().masblackleg
end
function autofarmmasblackleg()
    local folder4 = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Water Fighter [Lv. 1450]") then
        folder4 = game.ReplicatedStorage
    end
    for i,v in pairs(folder4:GetChildren()) do
        if v.Name == "Water Fighter [Lv. 1450]" then
            repeat wait()
                autoattack()
                sizepart(v)
                getgenv().psmasblackleg = v.HumanoidRootPart.Position
                if Settingss.atodeadstep then
                    equiptool("Black Leg")
                elseif Settingss.atosharkmanv2 then
                    equiptool("Fishman Karate")
                end
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
            until not v or not v.Parent or v.Humanoid.Health == 0  or (not Settingss.atodeadstep and not Settingss.atosharkmanv2 )
        end
    end
end

function bringmobmasblackleg()
    for i, v1 in pairs(game.workspace.Enemies:GetChildren()) do
        if v1.Name == "Water Fighter [Lv. 1450]" and isnetworkowner(v1.HumanoidRootPart)  and v1:FindFirstChild("Humanoid")  then
            v1.HumanoidRootPart.CFrame = CFrame.new(getgenv().psmasblackleg)
            sizepart(v1)
        end
    end
end




function autodeadthstep()
    if checkLevelBlackLeg() < 400 then
        return autofarmmasblackleg()
    end
	if game.ReplicatedStorage:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") or game.Workspace.Enemies:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") then
		local folder4 = game.workspace.Enemies
		if game.ReplicatedStorage:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") then
			folder4 = game.ReplicatedStorage
		end
		for i,v in pairs(folder4:GetChildren()) do
			if v.Name == "Awakened Ice Admiral [Lv. 1400] [Boss]" then
				repeat wait()
					autoattack()
					sizepart(v)
                        equiptool("Black Leg")
					toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                until not v or not v.Parent or v.Humanoid.Health == 0  or not Settingss.atodeadstep 
            end
        end
	else
		if checkkeylibrary() == nil and game.Workspace.Map.IceCastle.Hall.LibraryDoor.PhoeyuDoor.CanCollide == true then
            Teleport()
            tickprovip = tick()
            repeat wait()
            until tick() - tickprovip >= 20
		elseif checkkeylibrary() ~= nil and game.Workspace.Map.IceCastle.Hall.LibraryDoor.PhoeyuDoor.CanCollide == true then
			equiptool(checkkeylibrary())
			toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(6372.57, 302.195, -6838.97),CFrame.new(6372.57, 302.195, -6838.97))
		elseif game.Workspace.Map.IceCastle.Hall.LibraryDoor.PhoeyuDoor.CanCollide == false then
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyDeathStep")
		end
    end
end


function checkkeyWater()
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v.Name == "Water Key" then
			getgenv().namekeywater = v.Name
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v.Name == "Water Key" then
			getgenv().namekeywater = v.Name
		end
	end
	return getgenv().namekeywater
end
function autosharkmanv2()
    if checkLevelBlackLeg() < 400 then
        return autofarmmasblackleg()
    end
	if game.Workspace.Enemies:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") or game.ReplicatedStorage:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") then
		local folder4 = game.workspace.Enemies
		if game.ReplicatedStorage:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") then
			folder4 = game.ReplicatedStorage
		end
		for i,v in pairs(folder4:GetChildren()) do
			if v.Name == "Tide Keeper [Lv. 1475] [Boss]" then
				repeat wait()
					autoattack()
					sizepart(v)
					equiptool("Fishman Karate")
					toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                until not v or not v.Parent or v.Humanoid.Health == 0  or not Settingss.atosharkmanv2
            end
        end
	else
		if checkkeyWater() == nil and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true) == "I lost my house keys, could you help me find them? Thanks." then
            Teleport()
            tickprovip = tick()
            repeat wait()
            until tick() - tickprovip >= 20
		elseif checkkeyWater() ~= nil and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true) == "I lost my house keys, could you help me find them? Thanks." then
			equiptool(checkkeyWater())
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true)
		elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true) ~= "I lost my house keys, could you help me find them? Thanks." then
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate")
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true)
		end
    end
end




local skilldf = {
    ['Z'] = false,
    ['X'] = false,
    ['C'] = false,
    ['V'] = false,
    ['F'] = false
}

local function autoskilldf()
    if plr.Character:FindFirstChild("Humanoid") and plr.Character:FindFirstChild("Humanoid").Health > 0 then
    if plr:WaitForChild('Backpack'):FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value) then
        plr.Character.Humanoid:EquipTool(plr:WaitForChild('Backpack'):FindFirstChild(game.Players.LocalPlayer.Data.DevilFruit.Value))
    end

    if skilldf['Z'] then
        Manager:SendKeyEvent(true, 'Z', false, game)
        wait()
        Manager:SendKeyEvent(false, 'Z', false, game)
    end

    if skilldf['X'] then
        Manager:SendKeyEvent(true, 'X', false, game)
        wait()
        Manager:SendKeyEvent(false, 'X', false, game)
    end

    if skilldf['C'] then
        Manager:SendKeyEvent(true, 'C', false, game)
        wait()
        Manager:SendKeyEvent(false, 'C', false, game)
    end

    if skilldf['V'] then
        Manager:SendKeyEvent(true, 'V', false, game)
        wait()
        Manager:SendKeyEvent(false, 'V', false, game)
    end

    if skilldf['F'] then
        Manager:SendKeyEvent(true, 'F', false, game)
        wait()
        Manager:SendKeyEvent(false, 'F', false, game)
    end
    end
end
function checkforgun()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == "Gun" then
            getgenv().checkgunn = v.Name
        end
    end
    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("Tool") and  v.ToolTip == "Gun" then
            getgenv().checkgunn = v.Name
        end
    end
end

local function autoskillgun(v)
checkforgun()
if plr.Character:FindFirstChild("Humanoid") and plr.Character:FindFirstChild("Humanoid").Health > 0 then
if plr:WaitForChild('Backpack'):FindFirstChild(getgenv().checkgunn) then
    plr.Character.Humanoid:EquipTool(plr:WaitForChild('Backpack'):FindFirstChild(getgenv().checkgunn))
end
if skilldf['Z'] then
    Manager:SendKeyEvent(true, 'Z', false, game)
    wait()
    Manager:SendKeyEvent(false, 'Z', false, game)
end

if skilldf['X'] then
    Manager:SendKeyEvent(true, 'X', false, game)
    wait()
    Manager:SendKeyEvent(false, 'X', false, game)
end
onlygun(v)
end
end
local function masterygun()
pcall(function()
    if plr.Data.Level.Value >= 1500 and game.PlaceId == 4442272183 then
        getgenv().mob = "Water Fighter [Lv. 1450]"
    else
        getgenv().mob = checkquest()
    end
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild(getgenv().mob) then
        folder = game.ReplicatedStorage
    end
    for i,v in pairs(folder:GetChildren()) do
        if v.Name == getgenv().mob then
            if  v.Humanoid.Health > 0 then
                pcall(function()
                    repeat wait()
                        if v.Humanoid.Health / v.Humanoid.MaxHealth <= getgenv().limithp then
                            getgenv().psskill = v.HumanoidRootPart.Position
                            autoskillgun(v)
                            teleportautofarm(v)
                            checkproautofarm(v)
                        else
                            sizepart(v)
                            autoattack()
                            accessory()
                            getgenv().ps = v.HumanoidRootPart.Position
                            equiptool(Settingss.tool)
                            teleportautofarm(v)
                            checkproautofarm(v)
                        end
                    until v.Humanoid.Health == 0 or not v or not v.Parent or not getgenv().masteryGun
                    getgenv().pos = {}
                end)
            end
        end
    end
end)
end
local function mastery()
    pcall(function()
        if plr.Data.Level.Value >= 1500 and game.PlaceId == 4442272183 then
            getgenv().mob = "Water Fighter [Lv. 1450]"
        else
            getgenv().mob = checkquest()
        end
        local folder = game.workspace.Enemies
        if game.ReplicatedStorage:FindFirstChild(getgenv().mob) then
            folder = game.ReplicatedStorage
        end
        for i,v in pairs(folder:GetChildren()) do
            if v.Name == getgenv().mob then
                if  v.Humanoid.Health > 0 then
                    pcall(function()
                        repeat wait()
                            if v.Humanoid.Health / v.Humanoid.MaxHealth <= getgenv().limithp then
                                getgenv().psskill = v.HumanoidRootPart.Position
                                autoskilldf()
                                teleportautofarm(v)
                            else
                                sizepart(v)
                                autoattack()
                                getgenv().ps = v.HumanoidRootPart.Position
                                equiptool(Settingss.tool)
                                teleportautofarm(v)
                            end
                        until v.Humanoid.Health == 0 or not v or not v.Parent or not getgenv().mastery
                        getgenv().pos = {}
                    end)
                end
            end
        end
    end)
end

