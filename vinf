local freezewater = MSectionMics.AddToggle('Freeze Water',{Default_Value = false}, function(v)
    getgenv().freeze = v
    spawn(function()
        local char = plr.Character
        while getgenv().freeze do
            wait()
            if not char:FindFirstChild('cc') then
                local p = Instance.new('Part', char)
                p.Name = 'cc'
                p.Size = Vector3.new(5, 2, 5)
                p.CFrame = char.HumanoidRootPart.CFrame
                p.Transparency = 1
                p.CanCollide = true
                p.Anchored = true
            else
                char:FindFirstChild('cc').CFrame = CFrame.new(char.HumanoidRootPart.CFrame.X, -4.77,
                char.HumanoidRootPart.CFrame.Z)
            end
        end
    end)
end)
