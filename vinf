local ToggleClear = FSectionMics.AddToggle("Auto Delete Mob Froze",{Default_Value = Settingss.Froze},function(v)
    Settingss.Froze = v
    spawn(function()
        while Settingss.Froze and wait() do
            timerach = getgenv().minute * 60
            for i, v in pairs(game.workspace.Enemies:GetChildren()) do
                if v:FindFirstChild("Humanoid") and v.Humanoid.Health ~= 0 and v:FindFirstChild("HumanoidRootPart") then
                    wait(timerach)
                        v.Humanoid.Health = 0
                end
            end
        end
    end)
end)

