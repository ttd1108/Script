function checkhakikenprovip()
    if game:GetService("Players").LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("ImageLabel") then
        return true
        else
            return false
    end
end
local toggleatoken = FSectionMics.AddToggle("Auto Ken",{Default_Value = Settingss.atoken},function(v)
    Settingss.atoken = v 
    spawn(function()
        while wait() do
            if Settingss.atoken then
                pcall(function()
                    if checkhakikenprovip() == false then
                        game.Players.LocalPlayer.VisionRadius.Value = math.huge
                        game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game)
                        game:GetService("VirtualInputManager"):SendKeyEvent(false, "E", false, game)
                    end
                end)
            end
        end
    end)
end)

local toggleatoinvisible = FSectionMics.AddToggle("Auto Invisible", {Default_Value = Settingss.atoinvisible},function(v)
    Settingss.atoinvisible = v 
    spawn(function()
        while wait() do
            if Settingss.atoinvisible then
                pcall(function()
                    if Settingss.atoinvisible and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso") and not game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso"):FindFirstChild("Invisible") then 
                        local t = Instance.new("Part")
                        t.Name="LowerTorso"
                        local f = Instance.new("IntValue")
                        f.Name="Invisible"
                        game.Players.LocalPlayer.Character.LowerTorso:Destroy()
                        t.Parent = game.Players.LocalPlayer.Character
                        f.Parent = t
                        if game.Players.LocalPlayer.Character:FindFirstChild("CharacterReady") then
                            game.Players.LocalPlayer.Character.CharacterReady:Destroy()
                        end
                    end
                end)
            end
        end
    end)
end)
