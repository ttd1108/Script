local selectspeedtween = PSectionTP.AddSlider('Speed Tween',{Min = 200, Max = 300, Default_Value = Settingss.speedtween, Precise = false},function(v)
    Settingss.speedtween = v
end)
countisland = 0
local Lands = PSectionTP.AddDropdown('Teleport Lands',{List = Lands, Search = true, Selected = false},function(b)
    selectisland = b
end)
local toggletp = PSectionTP.AddToggle("Teleport Island",{Default_Value = false},function(v)
    getgenv().starttweenisland = v 
end)
function testconcac(u)
    local dist = math.huge
    getgenv().nameisland = nil
    for i,v in pairs(game.Workspace["_WorldOrigin"].Locations:GetChildren()) do
        if v.Name == u then
            for i,k in pairs(game:GetService("Workspace")["_WorldOrigin"].PlayerSpawns.Pirates:GetDescendants()) do
                if k.ClassName == "Part" then
                    local concac = (v.Position-k.Position).Magnitude 
                    if concac < dist then
                        dist = concac
                        getgenv().nameisland = k.CFrame
                    end
                end
            end
        end
    end
    return getgenv().nameisland
end

spawn(function()
    while wait() do
        if getgenv().starttweenisland then
            pcall(function()
                for i,v in pairs(game.Workspace["_WorldOrigin"].Locations:GetChildren()) do
                    if v.Name == selectisland then
                        repeat wait()
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,CFrame.new(testconcac(v.Name).X,game:GetService("Workspace").Camera["Water;"].CFrame.Y,testconcac(v.Name).Z).Position,CFrame.new(testconcac(v.Name).X,game:GetService("Workspace").Camera["Water;"].CFrame.Y,testconcac(v.Name).Z))
                        until (CFrame.new(testconcac(v.Name).X,game:GetService("Workspace").Camera["Water;"].CFrame.Y,testconcac(v.Name).Z).Position-plr.Character.HumanoidRootPart.Position).Magnitude < 150 or not getgenv().starttweenisland
                        repeat wait()
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,testconcac(v.Name).Position,testconcac(v.Name))
                        until (testconcac(v.Name).Position-plr.Character.HumanoidRootPart.Position).Magnitude < 100 or not not getgenv().starttweenisland
                        print("rac")
                    countisland = 0
                    tweenmain:Pause()
                    tweenmain:Cancel()
                    toggletp.SetStage(false)
                    end
                end
            end)
        end
    end
end)


