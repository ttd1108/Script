local MSectionBoss = FarmPage.AddSection("Boss Farm")
getgenv().allbossfarm = nil
local function farmallboss()
    for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
        if string.find(v.Name, ' %pBoss%p') then
            if getgenv().allbossfarm == nil then
                getgenv().allbossfarm = v.Name
            end
        end
    end
    for i, v in pairs(game.ReplicatedStorage:GetChildren()) do
        if string.find(v.Name, ' %pBoss%p') then
            if getgenv().allbossfarm == nil then
                getgenv().allbossfarm = v.Name
            end
        end
    end
        local folderfarmallboss = game.workspace.Enemies
        if game.ReplicatedStorage:FindFirstChild(getgenv().allbossfarm)  then
            folderfarmallboss = game.ReplicatedStorage
        end
    for i,v in pairs(folderfarmallboss:GetChildren()) do
        if v.Name == getgenv().allbossfarm and v.Humanoid.Health > 0 then
            pcall(function()
                getgenv().ps = v.HumanoidRootPart.CFrame
                repeat wait()
                    equiptool(Settingss.tool)
                    sizepart(v)
                    toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                    autoattack()
                until not v or not v.Parent or v.Humanoid.Health == 0 or not atofarmbossall
                getgenv().allbossfarm = nil
            end)
        end
    end
end

            
    
local togglefarmallboss = MSectionBoss.AddToggle("Farm All Boss",{Default_Value = false},function(v)
    atofarmbossall = v
    spawn(function()
        while wait() do
            if atofarmbossall then
                pcall(function()
                    farmallboss()
                end)
            end
        end
    end)
end)
