function CheckAcssesory() 
    for k,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
        if v:FindFirstChild("IsAccessory") then return true end
    end    
    return false
end

function accessory()
    if CheckAcssesory() == false and Settingss.atoaccessory then
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") and v.ToolTip == "Wear" then
                equiptool(v.Name)
                wait()
                game:GetService("VirtualUser"):CaptureController()
                game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
                wait()
                v.Parent = plr.Backpack
            end
        end
    end
end

