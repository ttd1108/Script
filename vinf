     }
     game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end

local function bringevilf()
    for i,v in pairs(game.Workspace:GetChildren()) do
        if v:IsA("Tool") and string.find(v.Name, "Fruit")  then
            v.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,50)
            wait(0.5)
            firetouchinterest(game.Players.LocalPlayer.Character.Head,v.Handle,0)
        end
    end
end


if not getgenv().ClearNotifications then
	game:GetService('RunService').Stepped:connect(function()
        if getgenv().clearnotf then 
            for i, v in pairs(plr.PlayerGui.Notifications:GetChildren()) do
                v.Visible = false
            end
        end
	end)
getgenv().ClearNotifications = true
end

local function ATTKStore(v)
    game.ReplicatedStorage.Remotes["CommF_"]:InvokeServer("getInventoryWeapons")
    game.ReplicatedStorage.Remotes["CommF_"]:InvokeServer("StoreItem", v)
end

local function ATTKSetup(v, path)
    for i,c in pairs(path:GetChildren()) do
        if not c.Name == v then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("MysteriousMan", "1")
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("MysteriousMan", "2")
        end
    end
end

local function ATTKLoad(v, x)
    if not x then
        CommF:InvokeServer("LoadItem", v)
    else
        CommF:InvokeServer("LoadItem", v)
        ATTKSetup(v, plr.Backpack)
        ATTKSetup(v, plr.Character)
    end
end



getgenv().conttk = false
local function AutoTTK(pat)
    for i, v in pairs(pat:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == "Sword" then
            if  v.Name ~= "Wando"
            and  v.Name ~= "Saddi"
            and  v.Name ~= "Shisui"
            and  v.Name ~= "True Triple Katana" then
                repeat wait()
                    if getgenv().Farm2 then
                        getgenv().Farm = false
                    end
                    if game.PlaceId == 7449423635  and getgenv().Farm == false  then -- sea 3
                        wait(4)
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(-218.92457580566, 6.729941368103, 5324.751953125),CFrame.new(-218.92457580566, 6.729941368103, 5324.751953125))
                    elseif game.PlaceId == 4442272183  and getgenv().Farm == false  then -- sea 2
                        wait(4)
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(125.37154388428, 21.915899276733, 2849.5302734375),CFrame.new(125.37154388428, 21.915899276733, 2849.5302734375))
                    end
                    ATTKStore(v.Name)
                    ATTKLoad("Wando")
                    if (game:GetService("Workspace").NPCs["Weapons Storage"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
                    ATTKStore(v.Name)
                    ATTKLoad("Wando")
                    wait(1)
                    getgenv().conttk = true
                    wait(2)
                    end
                until getgenv().conttk == true or not Settingss.AutoSw 
                getgenv().conttk = false
                if getgenv().Farm2 then
                    getgenv().Farm = true
                end
            end
            if v.Name == "Wando" then
                equiptool("Wando")
                if v.Level.Value >= 300 then
                    repeat wait()
                        if getgenv().Farm2 then
                            getgenv().Farm = false
                        end
                        if game.PlaceId == 7449423635  and getgenv().Farm == false  then
                            wait(4)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(-218.92457580566, 6.729941368103, 5324.751953125),CFrame.new(-218.92457580566, 6.729941368103, 5324.751953125))
                        elseif game.PlaceId == 4442272183  and getgenv().Farm == false  then
                            wait(4)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(125.37154388428, 21.915899276733, 2849.5302734375),CFrame.new(125.37154388428, 21.915899276733, 2849.5302734375))
                        end
                        ATTKStore(v.Name)
                        ATTKLoad("Saddi")
                        if (game:GetService("Workspace").NPCs["Weapons Storage"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
                        ATTKStore(v.Name)
                        ATTKLoad("Saddi")
                        wait(1)
                        getgenv().conttk = true
                        wait(2)
                        end
                    until getgenv().conttk == true or not Settingss.AutoSw
                    getgenv().conttk = false
                    if getgenv().Farm2 then
                        getgenv().Farm = true
                    end
                end
            elseif v.Name == "Saddi" then
                equiptool("Saddi")
                if v.Level.Value >= 300 then
                    repeat wait()
                        if getgenv().Farm2 then
                            getgenv().Farm = false
                        end
                        if game.PlaceId == 7449423635  and getgenv().Farm == false  then
                            wait(4)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(-218.92457580566, 6.729941368103, 5324.751953125),CFrame.new(-218.92457580566, 6.729941368103, 5324.751953125))
                        elseif game.PlaceId == 4442272183  and getgenv().Farm == false  then
                            wait(4)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(125.37154388428, 21.915899276733, 2849.5302734375),CFrame.new(125.37154388428, 21.915899276733, 2849.5302734375))
                        end
                        ATTKStore(v.Name)
                        ATTKLoad("Shisui")
                        if (game:GetService("Workspace").NPCs["Weapons Storage"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
                        ATTKStore(v.Name)
                        ATTKLoad("Shisui")
                        wait(1)
                        getgenv().conttk = true
                        wait(2)
                        end
                    until getgenv().conttk == true or not Settingss.AutoSw
                    getgenv().conttk = false
                    if getgenv().Farm2 then
                        getgenv().Farm = true
                    end
                end
            elseif v.Name == "Shisui" then
                equiptool("Shisui")
                if v.Level.Value >= 300 then
                    repeat wait()
                        if getgenv().Farm2 then
                            getgenv().Farm = false
                        end
                        if game.PlaceId == 7449423635 and getgenv().Farm == false  then
                            wait(4)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(-218.92457580566, 6.729941368103, 5324.751953125),CFrame.new(-218.92457580566, 6.729941368103, 5324.751953125))
                        elseif game.PlaceId == 4442272183 and getgenv().Farm == false    then
                            wait(4)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,Vector3.new(125.37154388428, 21.915899276733, 2849.5302734375),CFrame.new(125.37154388428, 21.915899276733, 2849.5302734375))
                        end
                        ATTKStore(v.Name)
                        ATTKLoad("True Triple Katana")
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("MysteriousMan", "2")
                        if (game:GetService("Workspace").NPCs["Weapons Storage"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
                        wait(1)
                        getgenv().conttk = true
                        wait(2)
                        end
                    until getgenv().conttk == true or not Settingss.AutoSw
                    getgenv().conttk = false
                    if getgenv().Farm2 then
                        getgenv().Farm = true
                    end
                end
            elseif v.Name == "True Triple Katana" then
                equiptool("True Triple Katana")
            else
            end
        end
    end
end

local function autoawkes()
    local args = {
        [1] = "Awakener",
        [2] = "Check"
     }

     game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
     local args = {
        [1] = "Awakener",
        [2] = "Awaken"
     }
     game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Awakener", "Awaken")
end

dfraid = {
    "Flame",
    "Ice",
    "Quake",
    "Light",
    "Dark",
    "String",
    "Rumble",
    "Magma",
    "Human: Buddha",
    "Sand"
}

local function AutoRaids()
    pcall(function()
        if not game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1")  and  plr.Data.Level.Value >= 1100 and not plr.PlayerGui.Notifications:FindFirstChild("NotificationTemplate") and not plr.PlayerGui.Main.Timer.Visible and not plr.Backpack:FindFirstChild("Special Microchip") then
            wait(3)
            if checkdevilfruit() == nil and Settingss.togglegetfruit then
                for i, v in pairs(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("getInventoryFruits")) do
                    for i1,v1 in pairs(v) do
                        if i1 == "Name" then
                            if not table.find(getgenv().tablefruit,v1) then
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("LoadFruit",v1)
                            end
                        end
                    end
                end
            end
            wait()
            Remote("RaidsNpc", "Check")
            Remote("RaidsNpc", "Select", Settingss.dfraid)
        end
        if plr.Backpack:FindFirstChild("Special Microchip") and not plr.PlayerGui.Main.Timer.Visible then
            if getgenv().Farm2 then
                getgenv().Farm = false
            end
            tweenmain:Pause()
            tweenmain:Cancel()
            getgenv().ps = nil
            if game.PlaceId == 7449423635 then
                wait(3)
                tweenmain:Pause()
                tweenmain:Cancel()
                    fireclickdetector(game:GetService("Workspace").Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
                oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
            end
            if game.PlaceId == 4442272183 then
                wait(3)
                tweenmain:Pause()
                tweenmain:Cancel()
                fireclickdetector(game:GetService("Workspace").Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
                oldfrag = game:GetService("Players").localPlayer.Data.Fragments.Value
            end
            wait(0.5)
        end
        
        for i,v in pairs(plr.PlayerGui.Notifications:GetDescendants()) do
            if v:IsA("TextLabel") then
                if string.find(v.Text,"1000") and getgenv().Farm2 then
                    tweenmain:Pause()
                    tweenmain:Cancel()
                    getgenv().Farm = true
                end
            end
        end

        if plr.PlayerGui.Main.Timer.Visible then
            if getgenv().Farm2 then
                getgenv().Farm = false
            end
            if game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame * CFrame.new(0,80,0))
             elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position,game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame * CFrame.new(0,80,0))
            end
        end
        if plr.PlayerGui.Main.Timer.Visible then
            for i,v in pairs(game.Workspace.Enemies:GetDescendants()) do
                if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and  (v.HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude < 400  and v.Humanoid.Health > 0 then
                    repeat wait()
                        v.Humanoid.Health = 0
                    until not v.Parent or v.Humanoid.Health <= 0 or not plr.PlayerGui.Main.Timer.Visible or not getgenv().AutoRaid
                end
            end
        end
    end)
end




local mobelite = {
    "Defeat  Diablo (0/1)",
    "Defeat  Deandre (0/1)",
    "Defeat  Urban (0/1)"
    }
local mobelitequest = {
    "Deandre [Lv. 1750]",
    "Diablo [Lv. 1750]",
    "Urban [Lv. 1750]"
}

local function autoelite()
    for i,v in pairs(game.ReplicatedStorage:GetChildren()) do
        if table.find(mobelitequest,v.Name) then
            getgenv().testelite = v.Name
        end
    end
    if game.ReplicatedStorage:FindFirstChild(getgenv().testelite) or game.Workspace.Enemies:FindFirstChild(getgenv().testelite) then
        if getgenv().Farm2 then
            getgenv().Farm = false
        end
        if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true and table.find(mobelite,game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text)  then
            local folder = game.workspace.Enemies
            if game.ReplicatedStorage:FindFirstChild(getgenv().testelite)  then
                folder = game.ReplicatedStorage
            end
            for i,v in pairs(folder:GetChildren()) do
                if v.Name == getgenv().testelite and v.Humanoid.Health > 0 then
                    pcall(function()
                        repeat wait()
                            sizepart(v)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                            equiptool(Settingss.tool)
                            autoattack()
                            accessory()
                        until not v.Parent or not v or game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false or not atoelite
                        tweencomplete(CFrame.new(-5417.83545, 313.063538, -2823.35913, 0.927179396, 0, 0.374617696, 0, 1, 0, -0.374617696, 0, 0.927179396) + Vector3.new(0,30,0))
                            if getgenv().Farm2 then
                                getgenv().Farm = true
                            end
                    end)
                end
            end
            else
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                wait(1)
                tweencomplete(CFrame.new(-5417.83545, 313.063538, -2823.35913, 0.927179396, 0, 0.374617696, 0, 1, 0, -0.374617696, 0, 0.927179396))
                wait(2)
                if (game:GetService("Workspace").NPCs["Elite Hunter"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude < 8 then
                    local args = {
                        [1] = "EliteHunter"
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end
        end
    end
end

local function autoquestcitizen()
    pcall(function()
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Forest Pirate [Lv. 1825]") then
        folder = game.ReplicatedStorage
    end
    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CitizenQuestProgress","Citizen") == 0 then
        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Forest Pirate") and string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "50") and game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
            for i,v in pairs(folder:GetChildren()) do
                if v.Name == "Forest Pirate [Lv. 1825]" and v.Humanoid.Health > 0 then
                    pcall(function()
                        repeat wait()
                            getgenv().bringcitizen = v.HumanoidRootPart.CFrame
                            sizepart(v)
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                            equiptool(Settingss.tool)
                            autoattack()
                            accessory()
                        until not v or not v.Parent or  v.Humanoid.Health == 0 or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false
                        tweencomplete(CFrame.new(-12441.5908, 331.488495, -7676.19727, -0.707134247, 0, 0.707079291, 0, 1, 0, -0.707079291, 0, -0.707134247))
                    end)
                end
            end
            else
                tweencomplete(CFrame.new(-12441.5908, 331.488495, -7676.19727, -0.707134247, 0, 0.707079291, 0, 1, 0, -0.707079291, 0, -0.707134247))
                local args = {
                [1] = "StartQuest",
                [2] = "CitizenQuest",
                [3] = 1
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CitizenQuestProgress","Citizen") == 1 then
        local foldercitizenboss = game.workspace.Enemies
        if game.ReplicatedStorage:FindFirstChild("Captain Elephant [Lv. 1875] [Boss]") then
            foldercitizenboss = game.ReplicatedStorage
        end
        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Captain Elephant") and string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "1") and game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
            for i,v in pairs(foldercitizenboss:GetChildren()) do
                if v.Name == "Captain Elephant [Lv. 1875] [Boss]" and v.Humanoid.Health > 0 then
                    pcall(function()
                        repeat wait()
                            sizepart(v)
                            equiptool(Settingss.tool)
                            autoattack()
                            accessory()
                            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                        until not v or not v.Parent or v.Humanoid.Health == 0 or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false
                    end)
                end
            end
            else
                tweencomplete(CFrame.new(-12441.5908, 331.488495, -7676.19727, -0.707134247, 0, 0.707079291, 0, 1, 0, -0.707079291, 0, -0.707134247))
                local args = {
                [1] = "StartQuest",
                [2] = "CitizenQuest",
                [3] = 1
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CitizenQuestProgress","Citizen") == 2 then
        tweencomplete(CFrame.new(-12513.8, 336.167, -9872.91))
        tweencomplete(CFrame.new(-12441.5908, 331.488495, -7676.19727, -0.707134247, 0, 0.707079291, 0, 1, 0, -0.707079291, 0, -0.707134247))
    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CitizenQuestProgress","Citizen") == 3 then
        if plr.Backpack:FindFirstChild("Fruit Bowl") or plr.Character:FindFirstChild("Fruit Bowl") then
            tweencomplete(CFrame.new(-10921.6, 623.505, -10270.3))
            else
                game.Workspace.BananaSpawner.Banana.Handle.CFrame = plr.Character.HumanoidRootPart.CFrame
                game.Workspace.AppleSpawner.Apple.Handle.CFrame = plr.Character.HumanoidRootPart.CFrame
                game.Workspace.PineappleSpawner.Pineapple.Handle.CFrame = plr.Character.HumanoidRootPart.CFrame
        end
        if  plr.Backpack:FindFirstChild("Banana") or  plr.Character:FindFirstChild("Banana") or  plr.Backpack:FindFirstChild("Pineapple") or  plr.Character:FindFirstChild("Pineapple") or  plr.Backpack:FindFirstChild("Apple") or  plr.Character:FindFirstChild("Apple") then
            tweencomplete(CFrame.new(-12441.5908, 331.488495, -7676.19727, -0.707134247, 0, 0.707079291, 0, 1, 0, -0.707079291, 0, -0.707134247))
        end
    end
    end)
end

local function bringcitizenquest()
    pcall(function()
        for i, v1 in pairs(game.workspace.Enemies:GetChildren()) do
            if v1.Name == "Forest Pirate [Lv. 1825]" then
                if v1:FindFirstChild("Humanoid") and v1.Humanoid.Health ~= 0 and isnetworkowner(v1.HumanoidRootPart) then
                    sizepart(v1)
                    v1.HumanoidRootPart.CFrame = getgenv().bringcitizen
                    autoattack()

                end
            end
        end
    end)
end

local function autoripindra()
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("rip_indra True Form [Lv. 5000] [Raid Boss]") then
        folder = game.ReplicatedStorage
    end
    if game.ReplicatedStorage:FindFirstChild("rip_indra True Form [Lv. 5000] [Raid Boss]") or game.Workspace.Enemies:FindFirstChild("rip_indra True Form [Lv. 5000] [Raid Boss]") then
        for i,v in pairs(folder:GetChildren()) do
            if v.Name == "rip_indra True Form [Lv. 5000] [Raid Boss]" and v.Humanoid.Health > 0 then
                pcall(function()
                    repeat wait()
                        sizepart(v)
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                        equiptool(Settingss.tool)
                        autoattack()
                        accessory()
                    until not v or not v.Parent or  v.Humanoid.Health == 0 or not Settingss.atoindra
                end)
            end
        end
    else
        if Settingss.atohopindra then
            wait(1)
            Teleport()
        end
    end
end



local function autotorch()
        if game:GetService("Workspace").Map.Waterfall.SecretRoom.Room.Door.Door.Hitbox:FindFirstChild("TouchInterest") and not  plr.Backpack:FindFirstChild("Holy Torch")  and  not plr.Character:FindFirstChild("Holy Torch") then
            if game:GetService("Workspace").Map.Turtle.QuestTorches.Torch1.Particles.Main.Enabled == false and game:GetService("Workspace").Map.Turtle.QuestTorches.Torch2.Particles.Main.Enabled == false and game:GetService("Workspace").Map.Turtle.QuestTorches.Torch3.Particles.Main.Enabled == false and game:GetService("Workspace").Map.Turtle.QuestTorches.Torch4.Particles.Main.Enabled == false and game:GetService("Workspace").Map.Turtle.QuestTorches.Torch5.Particles.Main.Enabled == false  then
                tweencomplete(game:GetService("Workspace").Map.Waterfall.SecretRoom.Room.Door.Door.Hitbox.CFrame)
            end
        elseif  plr.Backpack:FindFirstChild("Holy Torch") or plr.Character:FindFirstChild("Holy Torch")  then
            if game:GetService("Workspace").Map.Turtle.QuestTorches.Torch1.Particles.Main.Enabled == false then
                tweencomplete(CFrame.new(-10758.762695313, 412.54696655273, -9364.7099609375))
                wait(.5)
            elseif game:GetService("Workspace").Map.Turtle.QuestTorches.Torch2.Particles.Main.Enabled == false then
                tweencomplete(CFrame.new(-11681.762695313, 332.06646728516, -9477.296875))
                wait(.5)
            elseif game:GetService("Workspace").Map.Turtle.QuestTorches.Torch3.Particles.Main.Enabled == false then
                tweencomplete(CFrame.new(-12132.747070313, 519.79266357422, -10640.616210938))
                wait(.5)
            elseif game:GetService("Workspace").Map.Turtle.QuestTorches.Torch4.Particles.Main.Enabled == false then
                tweencomplete(CFrame.new(-13343.649414063, 486.87353515625, -6975.6005859375))
                wait(.5)
            elseif game:GetService("Workspace").Map.Turtle.QuestTorches.Torch5.Particles.Main.Enabled == false then
                tweencomplete(CFrame.new(-13479.262695313, 332.7214050293, -7930.5864257813))
            end
    end
end


