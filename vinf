        folder = game.ReplicatedStorage
    end
        if game.ReplicatedStorage:FindFirstChild("Longma [Lv. 2000] [Boss]") or game.Workspace.Enemies:FindFirstChild("Longma [Lv. 2000] [Boss]") then
            for i,v in pairs(folder:GetChildren()) do
                if v.Name == "Longma [Lv. 2000] [Boss]" and v.Humanoid.Health > 0 then
                    pcall(function()
