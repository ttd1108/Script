    getgenv().test = true
    wait()
    getgenv().test = false
end)
local togglefarmbounty = FSectionplayer.AddToggle("Farm Bounty (only Gun) [Risk]",{Default_Value = Settingss.atofarmbounty},function(v)
    Settingss.atofarmbounty = v 
    spawn(function()
        while wait() do
            if Settingss.atofarmbounty then
                pcall(function()
                    for i,v in pairs(game.Workspace.Characters:GetChildren()) do
                        if v:IsA("Model") then
                            if v.Name == ClosestPart()  and  not table.find(saveplayer,v.Name)  and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                                if not table.find(saveplayer,v.Name) then
                                    table.insert(saveplayer,v.Name)
                                end
                                tickfarmbountyyae = tick()
                                countgido = 0
                                repeat wait()
                                    godmode()
                                    TargetPlayer.SetText("Name Target :"..v.Name.."("..(v.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude..")")
                                    HealthPlayer.SetText("Health Player :"..v.Humanoid.Health)
                                    webhookfarmbounty(v.Name)
                                    if game.Players.LocalPlayer.PlayerGui.Main.HP.TextLabel.Text ~= "Health 100/100" and plr.Character.Humanoid.MaxHealth == 100 then
                                        spawn(function()
                                            onlygun(v)
                                        end)  
                                        --checkraidbounty(v)
                                        checksafezone(v)
                                        equipgun()
                                        checkconcaccailonmemay(v)
                                        plr.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
                                        if v.Humanoid.Health / v.Humanoid.MaxHealth <= 90/100 and countgido == 0 then
                                            countgido = countgido + 1
                                            tickfarmbountyyae = tick()
                                        end
                                    end
                                    
                                until tick()-tickfarmbountyyae >= 50 or not v or not v.Parent or v.Humanoid.Health == 0  or not Settingss.atofarmbounty or getgenv().test == true or checksafezone(v) == true or checkraidbounty(v) == true  or  getgenv().hopbounty == true 
                                getgenv().test = false
                                getgenv().cailonmemay = 0
                                getgenv().conbagiamay = 0
                                getgenv().concacbagiamay = 0
                                if not table.find(saveplayer,v.Name) then
                                    table.insert(saveplayer,v.Name)
                                end
                                getgenv().target = nil
                                print(#saveplayer)
                            end
                        end
                    end
                end)
            end
        end
    end)
end)

