local function uphakirainbown()
if string.find(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("HornedMan"), "Stone") then
    local folder = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Stone [Lv. 1550] [Boss]") then
        folder = game.ReplicatedStorage
    end
    if game.ReplicatedStorage:FindFirstChild("Stone [Lv. 1550] [Boss]") or game.workspace.Enemies:FindFirstChild("Stone [Lv. 1550] [Boss]") then
        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Stone") and string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "1") and game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
            for i,v in pairs(folder:GetChildren()) do
                if v.Name == "Stone [Lv. 1550] [Boss]" then
                    repeat wait()
                        accessory()
                        autoattack()
                        sizepart(v)
                        equiptool(Settingss.tool)
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                    until not v or not v.Parent or v.Humanoid.Health == 0 or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not atohakikenv2
                end
            end
            else
                tweencomplete(CFrame.new(-11893.7, 929.661, -8760.59))
                if (game:GetService("Workspace").NPCs["Horned Man"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("HornedMan","Bet")
                end
        end
    end
elseif string.find(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("HornedMan"), "Island Empress") then
    local folder1 = game.workspace.Enemies
    if game.ReplicatedStorage:FindFirstChild("Island Empress [Lv. 1675] [Boss]") then
        folder1 = game.ReplicatedStorage
    end
    if game.ReplicatedStorage:FindFirstChild("Island Empress [Lv. 1675] [Boss]") or game.workspace.Enemies:FindFirstChild("Island Empress [Lv. 1675] [Boss]") then
        if string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "Island Empress") and string.find(game.Players.LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, "1") and game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == true then
            for i,v in pairs(folder1:GetChildren()) do
                if v.Name == "Island Empress [Lv. 1675] [Boss]" then
                    repeat wait()
                        autoattack()
                        accessory()
                        sizepart(v)
                        equiptool(Settingss.tool)
                        toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position,v.HumanoidRootPart.Position,v.HumanoidRootPart.CFrame*GetPosPlus(v.HumanoidRootPart.CFrame))
                    until not v or not v.Parent or v.Humanoid.Health == 0 or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false or not atohakikenv2
                end
            end
            else
                tweencomplete(CFrame.new(-11893.7, 929.661, -8760.59))
                if (game:GetService("Workspace").NPCs["Horned Man"].HumanoidRootPart.Position-plr.Character.HumanoidRootPart.Position).Magnitude <= 8 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("HornedMan","Bet")
                end
        end
    end
